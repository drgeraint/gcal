<?xml version="1.0" encoding="UTF-8"?>
<!-- Element! --><!-- assessmentItem element! --><assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p1"
                xmlns:m="http://www.w3.org/1998/Math/MathML"
                xmlns:xi="http://www.w3.org/2001/XInclude"
                xmlns:xlink="http://www.w3.org/1999/xlink"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                toolName="Uniqurate"
                toolVersion="1.0"
                xml:lang="en"
                adaptive="true"
                timeDependent="false"
                identifier="Test07-011358-radToDegToRad"
                title="Convert between radians and degrees"
                xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqti_v2p1 imsqti_v2p1.xsd">
    <responseDeclaration identifier="HINTREQUEST" baseType="boolean" cardinality="single"/>
    <responseDeclaration identifier="SOLREQUEST" baseType="boolean" cardinality="single"/>
    <!-- RP uses float - would be better to use record as float input -->
    <responseDeclaration identifier="RESPONSE" cardinality="single" baseType="float"/>
    <!-- <responseDeclaration identifier="RESPONSE" cardinality="record"/> -->
    <outcomeDeclaration identifier="seenSolution" baseType="boolean" cardinality="single">
      <defaultValue>
         <value>false</value>
      </defaultValue>
   </outcomeDeclaration>
   <outcomeDeclaration identifier="seenHint" baseType="boolean" cardinality="single">
      <defaultValue>
         <value>false</value>
      </defaultValue>
   </outcomeDeclaration>
   <outcomeDeclaration identifier="ASKSOLUTION" baseType="identifier" cardinality="single">
      <defaultValue>
         <value>asksolution</value>
      </defaultValue>
   </outcomeDeclaration>
   <outcomeDeclaration identifier="ASKHINT" baseType="identifier" cardinality="single">
      <defaultValue>
         <value>askhint</value>
      </defaultValue>
   </outcomeDeclaration>
    <outcomeDeclaration baseType="float" cardinality="single" identifier="floatRespValue" />
    <outcomeDeclaration baseType="integer" cardinality="single" identifier="iRespDP" />
    <outcomeDeclaration baseType="integer" cardinality="single" identifier="iRespSF" />
    <outcomeDeclaration identifier="EMPTY" baseType="identifier" cardinality="single"/>
   <outcomeDeclaration identifier="FEEDBACK" baseType="identifier" cardinality="multiple"/>
    <outcomeDeclaration identifier="SOLPARTS" baseType="identifier" cardinality="multiple"/>
    <outcomeDeclaration identifier="SCORE" baseType="float" cardinality="single" normalMaximum="2.0"
                       normalMinimum="0">
        <defaultValue>
            <value>0
            </value>
        </defaultValue>
    </outcomeDeclaration>
    <templateDeclaration identifier="showQParts" baseType="identifier" cardinality="multiple"
                        mathVariable="false"
                        paramVariable="false"/>
    <templateDeclaration identifier="showSolParts" baseType="identifier" cardinality="multiple"
                        mathVariable="false"
                        paramVariable="false">
        <defaultValue>
            <value>NONE
            </value>
        </defaultValue>
    </templateDeclaration>
    <templateDeclaration identifier="fullMarks" baseType="float" cardinality="single"
                        mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="fMarksOff" baseType="float" cardinality="single"
                        mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="iQparts" baseType="integer" cardinality="single"
                        mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="iQrequire" baseType="integer" cardinality="single"
                        mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="iQtype" baseType="integer" cardinality="single" mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="iQnumnums" baseType="integer" cardinality="single"
                        mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration baseType="integer" cardinality="single" identifier="iQdecpl" mathVariable="false" paramVariable="false" />
    <templateDeclaration baseType="integer" cardinality="single" identifier="iDecsig" mathVariable="false" paramVariable="false" />
    <templateDeclaration identifier="oQues" cardinality="record" mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="iA" baseType="integer" cardinality="single" mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="iB" baseType="integer" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="iB1" baseType="integer" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="iB2" baseType="integer" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="sFromUnits" baseType="string" cardinality="single" mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="sToUnits" baseType="string" cardinality="single" mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="iType" baseType="integer" cardinality="single" mathVariable="true"
                        paramVariable="false"/>
    <templateDeclaration identifier="fArads" baseType="float" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="fAdegs" baseType="float" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="fQues" baseType="float" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="fAns" baseType="float" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="fAns6" baseType="float" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="fAns2" baseType="float" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="fBad1" baseType="float" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="fBad2" baseType="float" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateProcessing>
        <setTemplateValue identifier="fullMarks">
            <baseValue baseType="float">2.0</baseValue>
        </setTemplateValue>
        <setTemplateValue identifier="fMarksOff">
            <baseValue baseType="float">2.0</baseValue>
        </setTemplateValue>
        <setTemplateValue identifier="iQparts">
            <baseValue baseType="integer">1</baseValue>
        </setTemplateValue>
        <setTemplateValue identifier="iQrequire">
            <baseValue baseType="integer">5</baseValue>
        </setTemplateValue>
        <setTemplateValue identifier="iDecsig">
            <baseValue baseType="integer">1</baseValue>
        </setTemplateValue>
        <setTemplateValue identifier="iQdecpl">
            <baseValue baseType="integer">2</baseValue>
        </setTemplateValue>
        <setTemplateValue identifier="iQnumnums">
            <baseValue baseType="integer">1</baseValue>
        </setTemplateValue>
      <!-- -->
        <setTemplateValue identifier="iType">
            <randomInteger min="1" max="2"/>
        </setTemplateValue>
        <templateCondition>
            <templateIf>
                <equal toleranceMode="exact">
                    <variable identifier="iType"/>
                    <baseValue baseType="integer">1</baseValue>
                </equal>
                <setTemplateValue identifier="iA">
                    <randomInteger min="5" max="355"/>
                </setTemplateValue>
                <setTemplateValue identifier="fQues">
                    <integerToFloat>
                        <variable identifier="iA"/>
                    </integerToFloat>
                </setTemplateValue>
                <setTemplateValue identifier="fArads">
                    <roundTo roundingMode="decimalPlaces" figures="3">
                        <divide>
                            <product>
                                <variable identifier="iA"/>
                                <mathConstant name="pi"/>
                            </product>
                            <baseValue baseType="integer">180</baseValue>
                        </divide>
                    </roundTo>
                </setTemplateValue>
                <setTemplateValue identifier="fAns">
                    <variable identifier="fArads"/>
                </setTemplateValue>
                <setTemplateValue identifier="fBad1">
                    <product>
                        <divide>
                            <product>
                                <variable identifier="iA"/>
                                <baseValue baseType="integer">180</baseValue>
                            </product>
                            <mathConstant name="pi"/>
                        </divide>
                    </product>
                </setTemplateValue>
                <setTemplateValue identifier="showSolParts">
                    <multiple>
                        <baseValue baseType="identifier">Sol1</baseValue>
                    </multiple>
                </setTemplateValue>
            </templateIf>
            <templateElseIf>
                <equal toleranceMode="exact">
                    <variable identifier="iType"/>
                    <baseValue baseType="integer">2</baseValue>
                </equal>
                <setTemplateValue identifier="fArads">
                    <roundTo roundingMode="decimalPlaces" figures="3">
                        <randomFloat max="3.14" min="0.01"/>
                    </roundTo>
                </setTemplateValue>
                <setTemplateValue identifier="fQues">
                    <variable identifier="fArads"/>
                </setTemplateValue>
                <setTemplateValue identifier="fAdegs">
                    <divide>
                        <product>
                            <variable identifier="fArads"/>
                            <baseValue baseType="integer">180</baseValue>
                        </product>
                        <mathConstant name="pi"/>
                    </divide>
                </setTemplateValue>
                <setTemplateValue identifier="fAns">
                    <variable identifier="fAdegs"/>
                </setTemplateValue>
                <setTemplateValue identifier="fBad2">
                    <divide>
                        <product>
                            <variable identifier="fArads"/>
                            <mathConstant name="pi"/>
                        </product>
                        <baseValue baseType="integer">180</baseValue>
                    </divide>
                </setTemplateValue>
                <setTemplateValue identifier="showSolParts">
                    <multiple>
                        <baseValue baseType="identifier">Sol2</baseValue>
                    </multiple>
                </setTemplateValue>
            </templateElseIf>
        </templateCondition>
        
        <setTemplateValue identifier="sFromUnits">
            <index n="iType">
                <ordered>
                    <baseValue baseType="string">degrees</baseValue>
                    <baseValue baseType="string">radians</baseValue>
                </ordered>
            </index>
        </setTemplateValue>
        
        <setTemplateValue identifier="sToUnits">
            <index n="iType">
                <ordered>
                    <baseValue baseType="string">radians</baseValue>
                    <baseValue baseType="string">degrees</baseValue>
                </ordered>
            </index>
        </setTemplateValue>
        
        <setTemplateValue identifier="fAns6">
            <roundTo roundingMode="decimalPlaces" figures="6">
                <variable identifier="fAns"/>
            </roundTo>
        </setTemplateValue>
        
        <setTemplateValue identifier="fAns2">
            <roundTo roundingMode="decimalPlaces" figures="2">
                <variable identifier="fAns"/>
            </roundTo>
        </setTemplateValue>
        
    </templateProcessing>
    <itemBody>
        <div class="QTEXT">
            <p>Convert&#160;<printedVariable identifier="fQues"/>&#160;<printedVariable identifier="sFromUnits"/>&#160;to&#160;<printedVariable identifier="sToUnits"/>. 
            </p>
      </div>
            <p>Give your answer correct to 2 decimal places.</p>
        <div class="EQCENTRE">
         <table>
            <tbody>
                <tr>
                    <td>
                        <textEntryInteraction expectedLength="12" responseIdentifier="RESPONSE"/>
                    </td>
                    <td> 
                        <feedbackInline class="PART_OK" identifier="CORRECT" outcomeIdentifier="FEEDBACK"
                            showHide="show"> Correct </feedbackInline>
                        <feedbackInline class="PART_NO" identifier="WRONG" outcomeIdentifier="FEEDBACK" showHide="show"> Incorrect </feedbackInline>
                        <feedbackInline class="PART_NO" identifier="Bad1" outcomeIdentifier="FEEDBACK" showHide="show"> You should have multiplied by &#x3c0; and divided by 180. </feedbackInline>
                        <feedbackInline class="PART_NO" identifier="Bad2" outcomeIdentifier="FEEDBACK" showHide="show"> You should have multiplied by 180 and divided by &#x3c0;. </feedbackInline>
                    </td>
                </tr>
            </tbody>
         </table>
      </div>
        <feedbackBlock identifier="SOLUTION" outcomeIdentifier="FEEDBACK" showHide="show">
            <div class="EQCENTRE">
                <templateBlock templateIdentifier="showSolParts" identifier="Sol1" showHide="show" >
                    <m:math display="block">
                        <m:mtable>
                            <m:mtr>
                                <m:mtd columnalign="right">
                                    <m:mn>180</m:mn>
                                    <m:mo>&#xb0;</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="center">
                                    <m:mo>=</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="left">
                                    <m:mi>&#x3c0;</m:mi>
                                    <m:mtext>&#160;radians</m:mtext>
                                </m:mtd>
                            </m:mtr>
                            <m:mtr>
                                <m:mtd columnalign="right">
                                    <m:mn>1</m:mn>
                                    <m:mo>&#xb0;</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="center">
                                    <m:mo>=</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="left">
                                    <m:mfrac>
                                        <m:mi>&#x3c0;</m:mi>
                                        <m:mn>180</m:mn>
                                    </m:mfrac>
                                    <m:mtext>&#160;radians</m:mtext>
                                </m:mtd>
                            </m:mtr>
                            <m:mtr>
                                <m:mtd columnalign="right">
                                    <m:mi>fQues</m:mi>
                                    <m:mo>&#xb0;</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="center">
                                    <m:mo>=</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="left">
                                    <m:mi>fQues</m:mi>
                                    <m:mo>&#xd7;</m:mo>
                                    <m:mfrac>
                                        <m:mi>&#x3c0;</m:mi>
                                        <m:mn>180</m:mn>
                                    </m:mfrac>
                                    <m:mtext>&#160;radians</m:mtext>
                                </m:mtd>
                            </m:mtr>
                            <m:mtr>
                                <m:mtd columnalign="right">
                                    <m:mtext>&#160;</m:mtext>
                                </m:mtd>
                                <m:mtd columnalign="center">
                                    <m:mo>=</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="left">
                                    <m:mi>fAns2</m:mi>
                                    <m:mtext>&#160;radians</m:mtext>
                                </m:mtd>
                            </m:mtr>
                        </m:mtable>
                    </m:math>
                </templateBlock>
                <templateBlock templateIdentifier="showSolParts" identifier="Sol2" showHide="show" >
                    <m:math display="block">
                        <m:mtable>
                            <m:mtr>
                                <m:mtd columnalign="right">
                                    <m:mi>&#x3c0;</m:mi>
                                    <m:mtext>&#160;radians</m:mtext>
                                </m:mtd>
                                <m:mtd columnalign="center">
                                    <m:mo>=</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="left">
                                    <m:mn>180</m:mn>
                                    <m:mo>&#xb0;</m:mo>
                                </m:mtd>
                            </m:mtr>
                            <m:mtr>
                                <m:mtd columnalign="right">
                                    <m:mn>1</m:mn>
                                    <m:mtext>&#160;radian</m:mtext>
                                </m:mtd>
                                <m:mtd columnalign="center">
                                    <m:mo>=</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="left">
                                    <m:mfrac>
                                        <m:mrow>
                                            <m:mi>180</m:mi>
                                            <m:mo>&#xb0;</m:mo>
                                        </m:mrow>
                                        <m:mi>&#x3c0;</m:mi>
                                    </m:mfrac>
                                </m:mtd>
                            </m:mtr>
                            <m:mtr>
                                <m:mtd columnalign="right">
                                    <m:mi>fQues</m:mi>
                                    <m:mtext>&#160;radians</m:mtext>
                                </m:mtd>
                                <m:mtd columnalign="center">
                                    <m:mo>=</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="left">
                                    <m:mi>fQues</m:mi>
                                    <m:mo>&#xd7;</m:mo>
                                    <m:mfrac>
                                        <m:mrow>
                                            <m:mi>180</m:mi>
                                            <m:mo>&#xb0;</m:mo>
                                        </m:mrow>
                                        <m:mi>&#x3c0;</m:mi>
                                    </m:mfrac>
                                </m:mtd>
                            </m:mtr>
                            <m:mtr>
                                <m:mtd columnalign="right">
                                    <m:mtext>&#160;</m:mtext>
                                </m:mtd>
                                <m:mtd columnalign="center">
                                    <m:mo>=</m:mo>
                                </m:mtd>
                                <m:mtd columnalign="left">
                                    <m:mi>fAns2</m:mi>
                                    <m:mo>&#xb0;</m:mo>
                                </m:mtd>
                            </m:mtr>
                        </m:mtable>
                    </m:math>
                </templateBlock>
            </div>
            <p>So the answer is <printedVariable identifier="fAns2" format="%-10.2f"/>&#160;to 2 decimal places.</p>
        </feedbackBlock>
        <feedbackBlock identifier="HINT" outcomeIdentifier="FEEDBACK" showHide="show">
            <div class="QTEXT">
            <p>180 degrees = &#x3c0; radians</p>
         </div>
        </feedbackBlock>
        <div class="">
         <feedbackBlock showHide="show" identifier="askhint" outcomeIdentifier="ASKHINT">
            <p>
               <endAttemptInteraction responseIdentifier="HINTREQUEST" title="Show Hint"/>
            </p>
         </feedbackBlock>
         <feedbackBlock showHide="show" identifier="asksolution" outcomeIdentifier="ASKSOLUTION">
            <p>
               <endAttemptInteraction responseIdentifier="SOLREQUEST" title="Show Solution"/>
            </p>
         </feedbackBlock>
      </div>
    </itemBody>
    <!-- use equalRounded because number of dp or sig figs is also required to be correct 
     if user gives fewer dps or sig figs than required, judge wrong, if more judge partially correct 
     
     RESPONSE correct must be set to the UNROUNDED correct answer to get "TOOMANY" to display,
     if the rounded value is used it will judge it WRONG.
     
    Needs these variables:
    <responseDeclaration cardinality="record" identifier="RESPONSE" />
	<responseDeclaration identifier="HINTREQUEST" cardinality="single" baseType="boolean"/>
	<responseDeclaration identifier="SOLREQUEST" cardinality="single" baseType="boolean"/>
    
    <outcomeDeclaration baseType="float" cardinality="single" identifier="floatRespValue" />
    <outcomeDeclaration baseType="integer" cardinality="single" identifier="iRespDP" />
    <outcomeDeclaration baseType="integer" cardinality="single" identifier="iRespSF" />
	<outcomeDeclaration baseType="identifier" cardinality="multiple" identifier="FEEDBACK"/>
	<outcomeDeclaration baseType="identifier" cardinality="single" identifier="EMPTY"/>
	<outcomeDeclaration baseType="float" cardinality="single" identifier="SCORE" normalMaximum="2.0"
		normalMinimum="0.0">
		<defaultValue>
			<value>0.0</value>
		</defaultValue>
	</outcomeDeclaration>
	<outcomeDeclaration baseType="boolean" cardinality="single" identifier="seenSolution">
		<defaultValue>
			<value>false</value>
		</defaultValue>
	</outcomeDeclaration>
	<outcomeDeclaration baseType="boolean" cardinality="single" identifier="seenHint">
		<defaultValue>
			<value>false</value>
		</defaultValue>
	</outcomeDeclaration>
	<outcomeDeclaration baseType="identifier" cardinality="single" identifier="ASKHINT">
		<defaultValue>
			<value>askhint</value>
		</defaultValue>
	</outcomeDeclaration>
	<outcomeDeclaration baseType="identifier" cardinality="single" identifier="ASKSOLUTION">
		<defaultValue>
			<value>asksolution</value>
		</defaultValue>
	</outcomeDeclaration>
	
    <templateDeclaration baseType="integer" cardinality="single" identifier="iQdecpl" mathVariable="true" paramVariable="true" />
    <templateDeclaration baseType="integer" cardinality="single" identifier="iDecsig" mathVariable="true" paramVariable="true" />
    <templateDeclaration identifier="fAns" baseType="float" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    
    Feedback for CORRECT, TOOMANY, TOOFEW and WRONG may be provided, these are associated with the maths input box.
    Feedback THREETRIES, SEEN-SOLUTION and SEEN-HINT are needed to explain why the question
    shows the solution after 3 tries and changes SCORE to zero if the Solution was seen 
    or SCORE/2 if the Hint was used.
-->
    <responseProcessing>
        <setOutcomeValue identifier="FEEDBACK">
            <multiple>
                <variable identifier="EMPTY"/>
            </multiple>
        </setOutcomeValue>
        <responseCondition>
            <responseIf>
                <variable identifier="HINTREQUEST"/>
                <setOutcomeValue identifier="FEEDBACK">
                    <multiple>
                        <baseValue baseType="identifier">HINT</baseValue>
                    </multiple>
                </setOutcomeValue>
                <setOutcomeValue identifier="seenHint">
                    <baseValue baseType="boolean">true</baseValue>
                </setOutcomeValue>
            </responseIf>
            <responseElseIf>
                <variable identifier="SOLREQUEST"/>
                <setOutcomeValue identifier="FEEDBACK">
                    <multiple>
                        <baseValue baseType="identifier">SOLUTION</baseValue>
                    </multiple>
                </setOutcomeValue>
                <setOutcomeValue identifier="SOLPARTS">
                    <variable identifier="showSolParts"/>
                </setOutcomeValue>
                <setOutcomeValue identifier="seenSolution">
                    <baseValue baseType="boolean">true</baseValue>
                </setOutcomeValue>
                <setOutcomeValue identifier="completionStatus">
                    <baseValue baseType="identifier">completed</baseValue>
                </setOutcomeValue>
                <setOutcomeValue identifier="ASKHINT">
                    <baseValue baseType="identifier">null</baseValue>
                </setOutcomeValue>
                <setOutcomeValue identifier="ASKSOLUTION">
                    <baseValue baseType="identifier">null</baseValue>
                </setOutcomeValue>
            </responseElseIf>
            <responseElse>
                <setOutcomeValue identifier="FEEDBACK">
                    <!-- add initial member to feedback list -->
                    <multiple>
                        <baseValue baseType="identifier">JUDGING</baseValue>
                    </multiple>
                </setOutcomeValue>
                <responseCondition>
                    <responseIf>
                        <isNull>
                            <variable identifier="RESPONSE"/>
                        </isNull>
                        <setOutcomeValue identifier="SCORE">
                            <baseValue baseType="float">0.0</baseValue>
                        </setOutcomeValue>
                    </responseIf>
                    <responseElse>
                        <setOutcomeValue identifier="floatRespValue">
                            <variable identifier="RESPONSE"/>
                        </setOutcomeValue>
                        <responseCondition>
                            <responseIf>
                                <equal toleranceMode="exact">
                                    <round>
                                        <product>
                                            <variable identifier="RESPONSE"/>
                                            <power>
                                                <baseValue baseType="integer">10</baseValue>
                                                <variable identifier="iQdecpl"/>
                                            </power>
                                        </product>
                                    </round>
                                    <product>
                                        <variable identifier="RESPONSE"/>
                                        <power>
                                            <baseValue baseType="integer">10</baseValue>
                                            <variable identifier="iQdecpl"/>
                                        </power>
                                    </product>
                                </equal>
                                <setOutcomeValue identifier="iRespDP">
                                    <variable identifier="iQdecpl"/>
                                </setOutcomeValue>
                            </responseIf>
                            <responseElseIf>
                                <equal toleranceMode="exact">
                                    <round>
                                        <product>
                                            <variable identifier="RESPONSE"/>
                                            <power>
                                                <baseValue baseType="integer">10</baseValue>
                                                <subtract>
                                                    <variable identifier="iQdecpl"/>
                                                    <baseValue baseType="integer">1</baseValue>
                                                </subtract>
                                            </power>
                                        </product>
                                    </round>
                                    <product>
                                        <variable identifier="RESPONSE"/>
                                        <power>
                                            <baseValue baseType="integer">10</baseValue>
                                            <subtract>
                                                <variable identifier="iQdecpl"/>
                                                <baseValue baseType="integer">1</baseValue>
                                            </subtract>
                                        </power>
                                    </product>
                                </equal>
                                <setOutcomeValue identifier="iRespDP">
                                    <subtract>
                                        <variable identifier="iQdecpl"/>
                                        <baseValue baseType="integer">1</baseValue>
                                    </subtract>
                                </setOutcomeValue>
                            </responseElseIf>
                            <responseElseIf>
                                <equal toleranceMode="exact">
                                    <round>
                                        <product>
                                            <variable identifier="RESPONSE"/>
                                            <power>
                                                <baseValue baseType="integer">10</baseValue>
                                                <subtract>
                                                    <variable identifier="iQdecpl"/>
                                                    <baseValue baseType="integer">2</baseValue>
                                                </subtract>
                                            </power>
                                        </product>
                                    </round>
                                    <product>
                                        <variable identifier="RESPONSE"/>
                                        <power>
                                            <baseValue baseType="integer">10</baseValue>
                                            <subtract>
                                                <variable identifier="iQdecpl"/>
                                                <baseValue baseType="integer">2</baseValue>
                                            </subtract>
                                        </power>
                                    </product>
                                </equal>
                                <setOutcomeValue identifier="iRespDP">
                                    <subtract>
                                        <variable identifier="iQdecpl"/>
                                        <baseValue baseType="integer">2</baseValue>
                                    </subtract>
                                </setOutcomeValue>
                            </responseElseIf>
                            <responseElse>
                                <setOutcomeValue identifier="iRespDP">
                                    <baseValue baseType="integer">0</baseValue>
                                </setOutcomeValue>
                            </responseElse>
                        </responseCondition>
                        <responseCondition>
                            <responseIf>
                                <equalRounded roundingMode="decimalPlaces" figures="iQdecpl">
                                    <variable identifier="floatRespValue"/>
                                    <variable identifier="fAns"/>
                                </equalRounded>
                                <setOutcomeValue identifier="FEEDBACK">
                                    <multiple>
                                        <baseValue baseType="identifier">CORRECT</baseValue>
