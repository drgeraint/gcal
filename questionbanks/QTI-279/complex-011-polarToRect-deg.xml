    <templateProcessing>
        <setTemplateValue identifier="tDummy">
            <customOperator class="org.qtitools.mathassess.ScriptRule" ma:syntax="text/x-maxima">
                <baseValue baseType="string"><![CDATA[
                    
 iR : ev(random(9)+1,simp);
 
 choose(list) := list[1+random(length(list))];
 
 iThetar: ev((random(36)+1)*(2*random(2)-1)*5,simp);
 
 fsinTheta : ev(sin(iThetar*3.14159265359/180),numer,simp);
 
 fcosTheta : ev(cos(iThetar*3.14159265359/180),numer,simp);
 
 fA: ev(iR*fcosTheta,numer,simp);
 
 fB: ev(iR*fsinTheta,numer,simp);
 
 fabsa: ev(abs(fA),simp);
 
 fabsb: ev(abs(fB),simp);
 
 if fA>=0 then signa:maOperator("+") else signa:maOperator("-");
 
 if fB>=0 then signb:maOperator("+") else signb:maOperator("-");
 
]]></baseValue>
            </customOperator>
        </setTemplateValue>
