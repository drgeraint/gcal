                <setTemplateValue identifier="iB">
                    <randomInteger max="5" min="4"/>
                </setTemplateValue>
            </templateElseIf>
            <templateElseIf>
                <equal toleranceMode="exact">
                    <variable identifier="iA"/>
                    <baseValue baseType="integer">5</baseValue>
                </equal>
                <setTemplateValue identifier="iB">
                    <baseValue baseType="integer">5</baseValue>
                </setTemplateValue>
            </templateElseIf>
        </templateCondition>
        <setTemplateValue identifier="tdummy">
            <customOperator class="org.qtitools.mathassess.ScriptRule" ma:syntax="text/x-maxima">
                <baseValue baseType="string"><![CDATA[

 simp:true;

 mF1:ev((x-iA),simp);

 mF2:ev((x-iB),simp);

 mF:ev(mF1*mF2,simp);

 mQuad:ev(expand(mF),simp);

 

]]></baseValue>
            </customOperator>
        </setTemplateValue>
        <templateCondition>
            <templateIf>
                <equal toleranceMode="exact">
                    <variable identifier="iA"/>
                    <variable identifier="iB"/>
                </equal>
                <setCorrectResponse identifier="RESPONSE">
                    <baseValue baseType="identifier">OPTION5</baseValue>
                </setCorrectResponse>
            </templateIf>
            <templateElse>
                <setCorrectResponse identifier="RESPONSE">
                    <baseValue baseType="identifier">OPTION1</baseValue>
                </setCorrectResponse>
            </templateElse>
        </templateCondition>
        <setCorrectResponse identifier="RESPONSE1">
            <variable identifier="iA"/>
        </setCorrectResponse>
        <setCorrectResponse identifier="RESPONSE2">
            <variable identifier="iB"/>
        </setCorrectResponse>
    </templateProcessing>
    <itemBody class="">
        <div class="">
            <p class="">Find the interval(s) of&#160;<m:math id="mathML0">
                    <m:semantics>
                        <m:mi>x</m:mi>
                        <m:annotation encoding="SnuggleTeX">\[x\]</m:annotation>
                    </m:semantics>
            </m:math>&#160;for which&#160;<m:math id="mathML1">
                    <m:semantics>
                        <m:mrow>
                            <m:mi>mQuad</m:mi>
                            <m:mo>&lt;</m:mo>
                            <m:mn>0</m:mn>
                        </m:mrow>
                        <m:annotation encoding="SnuggleTeX">\[\qv{mQuad}&lt;0\]</m:annotation>
                    </m:semantics>
                </m:math>
            </p>
            <feedbackBlock class="" id="feedbackBlock0" identifier="part1" outcomeIdentifier="BODY"
                showHide="show">
                <div class="">
                    <p class=""/>
                    <p class="">First, select the form of your answer.</p>
                    <p class="">If the expression is true for all&#160;<m:math id="mathML2">
                            <m:semantics>
                                <m:mi>x</m:mi>
                                <m:annotation encoding="SnuggleTeX">\[x\]</m:annotation>
                            </m:semantics>
                        </m:math>, the answer is ALL. If there are no solutions, the answer is
                        NONE.</p>
                    <choiceInteraction responseIdentifier="RESPONSE" shuffle="true" maxChoices="1">
                        <simpleChoice identifier="OPTION1">
                            <m:math id="mathML3">
                                <m:semantics>
                                    <m:mrow>
                                        <m:mi>a</m:mi>
                                        <m:mo>&lt;</m:mo>
                                        <m:mi>x</m:mi>
                                        <m:mo>&lt;</m:mo>
                                        <m:mi>b</m:mi>
                                    </m:mrow>
                                    <m:annotation encoding="SnuggleTeX"
                                        >\[a&lt;x&lt;b\]</m:annotation>
                                </m:semantics>
                            </m:math>
                        </simpleChoice>
                        <simpleChoice identifier="OPTION2">
                            <m:math id="mathML4">
                                <m:semantics>
                                    <m:mrow>
                                        <m:mi>x</m:mi>
                                        <m:mo>&lt;</m:mo>
                                        <m:mi>a</m:mi>
                                    </m:mrow>
                                    <m:annotation encoding="SnuggleTeX">\[x&lt;a\]</m:annotation>
                                </m:semantics>
                            </m:math> , <m:math id="mathML5">
                                <m:semantics>
                                    <m:mrow>
                                        <m:mi>x</m:mi>
                                        <m:mo>&gt;</m:mo>
                                        <m:mi>b</m:mi>
                                    </m:mrow>
                                    <m:annotation encoding="SnuggleTeX">\[x&gt;b\]</m:annotation>
                                </m:semantics>
                            </m:math>
                        </simpleChoice>
                        <simpleChoice identifier="OPTION3">
                            <m:math id="mathML6">
                                <m:semantics>
                                    <m:mrow>
                                        <m:mi>x</m:mi>
                                        <m:mo>=</m:mo>
                                        <m:mi>a</m:mi>
                                    </m:mrow>
                                    <m:annotation encoding="SnuggleTeX">\[x=a\]</m:annotation>
                                </m:semantics>
                            </m:math>, <m:math id="mathML7">
                                <m:semantics>
                                    <m:mrow>
                                        <m:mi>x</m:mi>
                                        <m:mo>=</m:mo>
                                        <m:mi>b</m:mi>
                                    </m:mrow>
                                    <m:annotation encoding="SnuggleTeX">\[x=b\]</m:annotation>
                                </m:semantics>
                            </m:math>
                        </simpleChoice>
                        <simpleChoice identifier="OPTION4" fixed="true"> ALL </simpleChoice>
                        <simpleChoice identifier="OPTION5" fixed="true"> NONE </simpleChoice>
                    </choiceInteraction>
                </div>
            </feedbackBlock>
            <feedbackBlock id="feedbackBlock1" identifier="part2" outcomeIdentifier="BODY"
                showHide="show">
                <p class=""> </p>
            </feedbackBlock>
            <feedbackBlock id="feedbackBlock1" identifier="instructions2" outcomeIdentifier="BODY"
                showHide="show">
                <p class="">Complete the expression:</p>
            </feedbackBlock>
            <feedbackBlock id="feedbackBlock2" identifier="option1" outcomeIdentifier="BODY"
                showHide="show">
                <p class="">
                    <textEntryInteraction expectedLength="20" id="textEntryInteraction0"
                        label="mathInput" responseIdentifier="RESPONSE1"/>
                    <m:math id="mathML03">
                        <m:semantics>
                            <m:mrow>
                                <m:mo>&lt;</m:mo>
                                <m:mi>x</m:mi>
                                <m:mo>&lt;</m:mo>
                            </m:mrow>
                            <m:annotation encoding="SnuggleTeX">\[&lt;x&lt;\]</m:annotation>
