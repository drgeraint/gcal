                    <textEntryInteraction expectedLength="20" id="textEntryInteraction1"
                        label="mathInput" responseIdentifier="RESPONSE2"/>
                </p>
            </feedbackBlock>
            <feedbackBlock id="feedbackBlock3" identifier="option2" outcomeIdentifier="BODY"
