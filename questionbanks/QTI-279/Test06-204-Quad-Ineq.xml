    <templateProcessing>
        <setTemplateValue identifier="iA">
            <randomInteger max="5" min="-5"/>
        </setTemplateValue>
        <templateCondition>
            <templateIf>
                <equal toleranceMode="exact">
                    <variable identifier="iA"/>
                    <baseValue baseType="integer">-5</baseValue>
                </equal>
                <setTemplateValue identifier="iB">
                    <randomInteger max="5" min="-5"/>
                </setTemplateValue>
            </templateIf>
            <templateElseIf>
                <equal toleranceMode="exact">
                    <variable identifier="iA"/>
                    <baseValue baseType="integer">-4</baseValue>
                </equal>
                <setTemplateValue identifier="iB">
                    <randomInteger max="5" min="-4"/>
                </setTemplateValue>
            </templateElseIf>
            <templateElseIf>
                <equal toleranceMode="exact">
                    <variable identifier="iA"/>
                    <baseValue baseType="integer">-3</baseValue>
                </equal>
                <setTemplateValue identifier="iB">
                    <randomInteger max="5" min="-3"/>
                </setTemplateValue>
            </templateElseIf>
            <templateElseIf>
