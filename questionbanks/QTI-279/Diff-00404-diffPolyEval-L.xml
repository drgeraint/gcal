                                                    <m:mi>mX</m:mi>
                                                    <m:mo>=</m:mo>
                                                    <m:mi>mValX</m:mi>
                                                </m:mrow>
                                                <m:annotation encoding="SnuggleTeX">\[\qv{mX} =
                                                    \qv{mValX}\]</m:annotation>
