    <templateProcessing>
        <setTemplateValue identifier="tDummy">
            <customOperator class="org.qtitools.mathassess.ScriptRule" ma:syntax="text/x-maxima">
                <baseValue baseType="string"><![CDATA[

 simp:true;

 s1 : make_random_state(true)$ set_random_state(s1);

 mValX: ev((random(4)+1)*(random(2)*2-1),simp);

 iA1: ev((random(6)+2)*(random(2)*2-1),simp);

 iA2: ev((random(6)+2)*(random(2)*2-1),simp);

 iA3: ev((random(6)+2)*(random(2)*2-1),simp);

 iA4: ev((random(6)+2)*(random(2)*2-1),simp);

 iB1: ev(3*iA1,simp);

 iB2: ev(2*iA2,simp);

 iB3: ev(iA3,simp);

 iB4: 0;

 choose(list) := list[1+random(length(list))];

 varNames:[a,b,c,h,k,m,n,p,q,r,s,t,u,v,w,x,y,z];

 mX:choose(varNames);

 varNames: delete(mX,varNames);

 mY:choose(varNames);

 mQues: ev(iA1*mX^3+iA2*mX^2+iA3*mX+iA4,simp);

 mDeriv: diff(mQues,mX);
 
 iAbsB2:abs(iB2);
 iAbsB3:abs(iB3);
 if is(iB2>=0)=true then mSignB2:maOperator("+") else mSignB2:maOperator("-");
 if is(iB3>=0)=true then mSignB3:maOperator("+") else mSignB3:maOperator("-");

 mAns: ev(subst(mValX,mX,mDeriv),simp);

 mInt: integrate(mQues,mX);

 

]]></baseValue>
            </customOperator>
        </setTemplateValue>
    </templateProcessing>
    <itemBody class="">
        <div class="">
            <p>Given that</p>
            <div class="">
                <m:math display="block" id="mathML0">
                    <m:semantics>
                        <m:mrow>
                            <m:mi>mY</m:mi>
                            <m:mo>=</m:mo>
                            <m:mi>mQues</m:mi>
                        </m:mrow>
                        <m:annotation encoding="SnuggleTeX">\[f(\qv{mX}) =
                            \qv{mQues}\]</m:annotation>
                    </m:semantics>
                </m:math>
            </div>
            <p>Find the value of&#160;<m:math id="mathML41" display="block">
                <m:semantics>
                    <m:mrow>
                        <m:mfrac>
                            <m:mrow>
                                <m:mi>d</m:mi>
                                <m:mi>mY</m:mi>
                            </m:mrow>
                            <m:mrow>
                                <m:mi>d</m:mi>
                                <m:mi>mX</m:mi>
                            </m:mrow>
                        </m:mfrac>
                    </m:mrow>
                    <m:annotation encoding="SnuggleTeX">\[\frac{d\qv{mY}}{d\qv{mX}}\]</m:annotation>
                </m:semantics>
            </m:math>&#160;when&#160;<m:math id="mathML02">
                <m:semantics>
                    <m:mrow>
                        <m:mi>mX</m:mi>
                        <m:mo>=</m:mo>
                        <m:mi>mValX</m:mi>
                    </m:mrow>
                    <m:annotation encoding="SnuggleTeX">\[\qv{mX} =
                        \qv{mValX}\]</m:annotation>
                </m:semantics>
            </m:math></p>
            <div class="">
                <table class="">
                    <tbody class="">
                        <tr class="">
                            <td class="">
                                <m:math id="mathML1" display="block">
                                    <m:semantics>
                                        <m:mrow>
                                            <m:mfrac>
                                                <m:mrow>
                                                    <m:mi>d</m:mi>
                                                    <m:mi>mY</m:mi>
                                                </m:mrow>
                                                <m:mrow>
                                                    <m:mi>d</m:mi>
                                                    <m:mi>mX</m:mi>
                                                </m:mrow>
                                            </m:mfrac>
                                            <m:mo>(</m:mo>
                                                <m:mi>mX</m:mi>
                                                <m:mo>=</m:mo>
                                                <m:mi>mValX</m:mi>
                                            <m:mo>)</m:mo>
                                        </m:mrow>
                                        <m:annotation encoding="SnuggleTeX">\[\frac{d\qv{mY}}{d\qv{mX}}(\qv{mX}=\qv{mValX}) =
                                            \]</m:annotation>
                                    </m:semantics>
                                </m:math>
                            </td>
                            <td class="">
                                <customInteraction
