	<outcomeDeclaration baseType="identifier" cardinality="single" identifier="ASKHINT">
		<defaultValue>
			<value>askhint</value>
		</defaultValue>
	</outcomeDeclaration>
	<outcomeDeclaration baseType="identifier" cardinality="single" identifier="ASKSOLUTION">
		<defaultValue>
			<value>asksolution</value>
		</defaultValue>
	</outcomeDeclaration>
    <outcomeDeclaration baseType="float" cardinality="single" identifier="floatRespValue1" />
    <outcomeDeclaration baseType="integer" cardinality="single" identifier="iRespDP1" />
    <outcomeDeclaration baseType="string" cardinality="single" identifier="sResp1" />
    <outcomeDeclaration baseType="integer" cardinality="single" identifier="iRespSF1" />
    <outcomeDeclaration baseType="float" cardinality="single" identifier="floatRespValue2" />
    <outcomeDeclaration baseType="integer" cardinality="single" identifier="iRespDP2" />
    <outcomeDeclaration baseType="string" cardinality="single" identifier="sResp2" />
    <outcomeDeclaration baseType="integer" cardinality="single" identifier="iRespSF2" />
    <templateDeclaration identifier="fullMarks" baseType="float" cardinality="single"
        mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="fMarksOff" baseType="float" cardinality="single"
        mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="iQparts" baseType="integer" cardinality="single"
        mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="iQrequire" baseType="integer" cardinality="single"
        mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="iQtype" baseType="integer" cardinality="single" mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration identifier="iQnumnums" baseType="integer" cardinality="single"
        mathVariable="true"
        paramVariable="false"/>
    <templateDeclaration baseType="integer" cardinality="single" identifier="iQdecpl" mathVariable="false" paramVariable="false" />
    <templateDeclaration baseType="integer" cardinality="single" identifier="iDecsig" mathVariable="false" paramVariable="false" />
    <templateDeclaration baseType="boolean" cardinality="single" identifier="keepRandomSeed"
		mathVariable="false" paramVariable="false"/>
	<templateDeclaration baseType="integer" cardinality="single" identifier="iRandomSeed"
		mathVariable="false" paramVariable="false"/>
	<templateDeclaration identifier="oExpr" cardinality="record" mathVariable="true"/>
	<templateDeclaration identifier="tDummy" cardinality="single" baseType="boolean"/>
    <templateDeclaration identifier="iA" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="iB" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="iC" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="iD" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="iAC" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="iBSq" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="iDiscr" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="iMinusB" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="iTwoA" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="iFourAC" cardinality="single" baseType="integer" mathVariable="true"/>
    <templateDeclaration identifier="fSqrtDiscr" cardinality="single" baseType="float" mathVariable="true"/>
    <templateDeclaration identifier="fRoot1" cardinality="single" baseType="float" mathVariable="true"/>
    <templateDeclaration identifier="fRoot2" cardinality="single" baseType="float" mathVariable="true"/>
    <templateDeclaration identifier="mFac1" cardinality="record" mathVariable="true"/>
	<templateDeclaration identifier="mFac2" cardinality="record" mathVariable="true"/>
	<templateDeclaration identifier="mCX" cardinality="record" mathVariable="true"/>
	<templateDeclaration identifier="mQues" cardinality="record" mathVariable="true"/>
	<templateDeclaration identifier="mAns" cardinality="record" mathVariable="true"/>
	<templateProcessing>
	    <setTemplateValue identifier="fullMarks">
	        <baseValue baseType="float">2.0</baseValue>
	    </setTemplateValue>
	    <setTemplateValue identifier="fMarksOff">
	        <baseValue baseType="float">2.0</baseValue>
	    </setTemplateValue>
	    <setTemplateValue identifier="iQparts">
	        <baseValue baseType="integer">1</baseValue>
	    </setTemplateValue>
	    <setTemplateValue identifier="iQrequire">
	        <baseValue baseType="integer">5</baseValue>
	    </setTemplateValue>
	    <setTemplateValue identifier="iDecsig">
	        <baseValue baseType="integer">1</baseValue>
	    </setTemplateValue>
	    <setTemplateValue identifier="iQdecpl">
	        <baseValue baseType="integer">2</baseValue>
	    </setTemplateValue>
	    <setTemplateValue identifier="iQnumnums">
	        <baseValue baseType="integer">1</baseValue>
	    </setTemplateValue>
	    <setTemplateValue identifier="iA">
	        <randomInteger min="1" max="5"/>
	    </setTemplateValue>
	    <setTemplateValue identifier="iB">
	        <product>
	            <randomInteger min="1" max="10"/>
	            <randomInteger min="-1" max="1" step="2"/>
	        </product>
	    </setTemplateValue>
	    <setTemplateValue identifier="iC">
	        <product>
	            <randomInteger min="1" max="10"/>
	            <randomInteger min="-1" max="1" step="2"/>
	        </product>
	    </setTemplateValue>
	    <setTemplateValue identifier="iDiscr">
	        <subtract>
	            <round>
	                <power>
    	                <variable identifier="iB"/>
    	                <baseValue baseType="integer">2</baseValue>
    	            </power>
	            </round>
	            <product>
	                <baseValue baseType="integer">4</baseValue>
	                <variable identifier="iA"/>
	                <variable identifier="iC"/>
	            </product>
	        </subtract>
	    </setTemplateValue>
	    <templateConstraint>
	        <gt>
	            <variable identifier="iDiscr"/>
	            <baseValue baseType="integer">0</baseValue>
	        </gt>
	    </templateConstraint>
	    <templateConstraint>
	        <not>
	            <equal toleranceMode="exact">
	                <variable identifier="iDiscr"/>
	                <power>
	                    <power>
	                        <variable identifier="iDiscr"/>
	                        <baseValue baseType="float">0.5</baseValue>
	                    </power>
	                    <baseValue baseType="integer">2</baseValue>
	                </power>
	            </equal>
	        </not>
	    </templateConstraint>
	    <setTemplateValue identifier="iAC">
	        <product>
	            <variable identifier="iA"/>
	            <variable identifier="iC"/>
	        </product>
	    </setTemplateValue>
	    <setTemplateValue identifier="iMinusB">
	        <product>
	            <baseValue baseType="integer">-1</baseValue>
	            <variable identifier="iB"/>
	        </product>
	    </setTemplateValue>
	    <setTemplateValue identifier="iBSq">
	        <product>
	            <variable identifier="iB"/>
	            <variable identifier="iB"/>
	        </product>
	    </setTemplateValue>
	    <setTemplateValue identifier="iFourAC">
	        <product>
	            <baseValue baseType="integer">4</baseValue>
	            <variable identifier="iAC"/>
	        </product>
	    </setTemplateValue>
	    <setTemplateValue identifier="iTwoA">
	        <product>
	            <baseValue baseType="integer">2</baseValue>
	            <variable identifier="iA"/>
	        </product>
	    </setTemplateValue>
	    <setTemplateValue identifier="fRoot1">
	        <roundTo roundingMode="decimalPlaces" figures="2">
	            <divide>
     	            <sum>
     	                <variable identifier="iMinusB"/>
     	                <power>
     	                    <variable identifier="iDiscr"/>
     	                    <baseValue baseType="float">0.5</baseValue>
     	                </power>
     	            </sum>
     	            <variable identifier="iTwoA"/>
     	        </divide>
	        </roundTo>
	    </setTemplateValue>
	    <setTemplateValue identifier="fRoot2">
	        <roundTo roundingMode="decimalPlaces" figures="2">
	            <divide>
       	            <subtract>
       	                <variable identifier="iMinusB"/>
       	                <power>
       	                    <variable identifier="iDiscr"/>
       	                    <baseValue baseType="float">0.5</baseValue>
       	                </power>
       	            </subtract>
       	            <variable identifier="iTwoA"/>
       	        </divide>
	        </roundTo>
	    </setTemplateValue>
	    <setTemplateValue identifier="tDummy">
			<customOperator class="org.qtitools.mathassess.ScriptRule" ma:syntax="text/x-maxima">
				<baseValue baseType="string">
					<![CDATA[
			mQues:ev(iA*x^2+iB*x+iC,simp);
			]]>
				</baseValue>
			</customOperator>
		</setTemplateValue>
	</templateProcessing>
	<itemBody>
		<p> Solve this quadratic equation: </p>
		<p>
			<m:math display="block">
			    <m:mi>mQues</m:mi>
			    <m:mo>=</m:mo>
			    <m:mn>0</m:mn>
			</m:math>
		</p>
	    <div class="EQCENTRE">
	        <table>
	            <tbody>
	                <tr>
	                    <td>
	                        <em>x = </em>
	                    </td>
	                    <td>
	                        <textEntryInteraction expectedLength="12" responseIdentifier="RESPONSE1"/>&#160;
	                    </td>
	                </tr>
	                <tr>
	                    <td> 
	                        <feedbackInline class="PART_OK" identifier="CORRECT1" outcomeIdentifier="FEEDBACK"
	                            showHide="show"> Your <printedVariable identifier="sResp1"/> answer is correct. </feedbackInline>
	                        <feedbackInline class="TOOFEW" identifier="TOOFEW1" outcomeIdentifier="FEEDBACK" showHide="show">
	                            You have not rounded your <printedVariable identifier="sResp1"/> answer correctly.
	                        </feedbackInline>
	                        <feedbackInline class="PART_NO" identifier="WRONG1" outcomeIdentifier="FEEDBACK" showHide="show"> Your <printedVariable identifier="sResp1"/> answer is incorrect. </feedbackInline>
	                    </td>
	                </tr>
	            </tbody>
	        </table>
	    </div>
	    <div class="EQCENTRE">
	        <table>
	            <tbody>
	                <tr>
	                    <td>or <em>x = </em></td>
	                    <td>
	                        <textEntryInteraction expectedLength="12" responseIdentifier="RESPONSE2"/>
	                    </td>
	                </tr>
	                <tr>
	                    <td> 
	                        <feedbackInline class="PART_OK" identifier="CORRECT2" outcomeIdentifier="FEEDBACK"
	                            showHide="show"> Your <printedVariable identifier="sResp2"/> answer is correct </feedbackInline>
	                        <feedbackInline class="TOOFEW" identifier="TOOFEW2" outcomeIdentifier="FEEDBACK" showHide="show">
	                            You have not rounded your <printedVariable identifier="sResp2"/> answer correctly.
	                        </feedbackInline>
	                        <feedbackInline class="PART_NO" identifier="WRONG2" outcomeIdentifier="FEEDBACK" showHide="show"> Your <printedVariable identifier="sResp2"/> answer is incorrect </feedbackInline>
	                    </td>
	                </tr>
	            </tbody>
	        </table>
	    </div>
	    <feedbackBlock id="feedbackBlock4" identifier="THREETRIES"
	        outcomeIdentifier="FEEDBACK" showHide="show">
	        <p> You have had three attempts at answering this question, so here is the
	            solution: </p>
	    </feedbackBlock>
	    <feedbackBlock showHide="show" outcomeIdentifier="FEEDBACK" identifier="SOLUTION">
		    <p>Using the quadratic formula, with</p>
		    <div>
		        <m:math display="block" id="mathML0">
		            <m:semantics>
		                <m:mrow>
		                    <m:mi>a</m:mi>
		                    <m:mo>=</m:mo>
		                    <m:mi>iA</m:mi>
		                </m:mrow>
		                <m:annotation encoding="SnuggleTeX">\[a=\qv{iA}\]</m:annotation>
		            </m:semantics>
		        </m:math>
		    </div>
		    <div>
		        <m:math display="block" id="mathML1">
		            <m:semantics>
		                <m:mrow>
		                    <m:mi>b</m:mi>
		                    <m:mo>=</m:mo>
		                    <m:mi>iB</m:mi>
		                </m:mrow>
		                <m:annotation encoding="SnuggleTeX">\[b=\qv{iB}\]</m:annotation>
		            </m:semantics>
		        </m:math>
		    </div>
		    <div>
		        <m:math display="block" id="mathML11">
		            <m:semantics>
		                <m:mrow>
		                    <m:mi>c</m:mi>
		                    <m:mo>=</m:mo>
		                    <m:mi>iC</m:mi>
		                </m:mrow>
		                <m:annotation encoding="SnuggleTeX">\[c=\qv{iC}\]</m:annotation>
		            </m:semantics>
		        </m:math>
		    </div>
		    <div>
		        <m:math display="block" id="mathML2">
		            <m:semantics>
		                <m:mtable>
		                    <m:mtr>
		                        <m:mtd columnalign="right">
		                            <m:mi>x</m:mi>
		                        </m:mtd>
		                        <m:mtd columnalign="right"/>
		                        <m:mtd columnalign="center">
		                            <m:mo>=</m:mo>
		                        </m:mtd>
		                        <m:mtd columnalign="left">
		                            <m:mfrac>
      		                            <m:mrow>
      		                                <m:mi>iMinusB</m:mi>
      		                                <m:mo>&#xb1;</m:mo>
      		                                <m:msqrt>
      		                                    <m:msup>
      		                                        <m:mi>iB</m:mi>
      		                                        <m:mn>2</m:mn>
      		                                    </m:msup>
      		                                    <m:mo>-</m:mo>
      		                                    <m:mfenced>
      		                                        <m:mn>4</m:mn>
      		                                        <m:mo>&#xd7;</m:mo>
      		                                        <m:mi>iA</m:mi>
      		                                        <m:mo>&#xd7;</m:mo>
      		                                        <m:mi>iC</m:mi>
      		                                    </m:mfenced>
      		                                </m:msqrt>
      		                            </m:mrow>
		                                <m:mrow>
		                                    <m:mn>2</m:mn>
		                                    <m:mo>&#xd7;</m:mo>
		                                    <m:mi>iA</m:mi>
		                                </m:mrow>
		                            </m:mfrac>
		                        </m:mtd>
		                    </m:mtr>
		                    <m:mtr>
		                        <m:mtd columnalign="right">
		                            <m:mi> </m:mi>
		                        </m:mtd>
		                        <m:mtd columnalign="right"/>
		                        <m:mtd columnalign="center">
		                            <m:mo>=</m:mo>
		                        </m:mtd>
		                        <m:mtd columnalign="left">
		                            <m:mfrac>
		                                <m:mrow>
		                                    <m:mi>iMinusB</m:mi>
		                                    <m:mo>&#xb1;</m:mo>
		                                    <m:msqrt>
		                                        <m:mi>iBSq</m:mi>
		                                        <m:mo>-</m:mo>
		                                        <m:mfenced>
		                                            <m:mi>iFourAC</m:mi>
		                                        </m:mfenced>
		                                    </m:msqrt>
		                                </m:mrow>
		                                <m:mrow>
		                                    <m:mi>iTwoA</m:mi>
		                                </m:mrow>
		                            </m:mfrac>
		                        </m:mtd>
		                    </m:mtr>
		                    <m:mtr>
		                        <m:mtd columnalign="right">
		                            <m:mi> </m:mi>
		                        </m:mtd>
		                        <m:mtd columnalign="right"/>
		                        <m:mtd columnalign="center">
		                            <m:mo>=</m:mo>
		                        </m:mtd>
		                        <m:mtd columnalign="left">
		                            <m:mfrac>
		                                <m:mrow>
		                                    <m:mi>iMinusB</m:mi>
		                                    <m:mo>&#xb1;</m:mo>
		                                    <m:msqrt>
		                                       <m:mi>iDiscr</m:mi>
		                                    </m:msqrt>
		                                </m:mrow>
		                                <m:mrow>
		                                    <m:mi>iTwoA</m:mi>
		                                </m:mrow>
		                            </m:mfrac>
		                        </m:mtd>
		                    </m:mtr>
		                    <m:mtr>
		                        <m:mtd columnalign="right">
		                            <m:mi>x</m:mi>
		                        </m:mtd>
		                        <m:mtd columnalign="right"/>
		                        <m:mtd columnalign="center">
		                            <m:mo>=</m:mo>
		                        </m:mtd>
		                        <m:mtd columnalign="left">
		                            <m:mi>fRoot1</m:mi>
		                            <m:mtext>&#160;or&#160;</m:mtext>
		                            <m:mi>fRoot2</m:mi>
		                        </m:mtd>
		                    </m:mtr>
		                </m:mtable>
		                <m:annotation encoding="SnuggleTeX">\begin{eqnarray}x 
		                    &amp;=&amp; \frac{\qv{iMinusB}\sqrt{\qv{iBSq}-4\times \qv{iA}\times \qv{iC}}{2\times \qv{iA}}
		                    &amp;=&amp; \frac{\qv{iMinusB}\sqrt{\qv{iBSq}-\qv{iFourAC}}{\qv{iTwoA}}
		                    &amp;=&amp; \qv{fRoot1} \mbox{or} \qv{fRoot2}
		                    \end</m:annotation>
		            </m:semantics>
		        </m:math>
		    </div>
		</feedbackBlock>
		<feedbackBlock showHide="show" outcomeIdentifier="FEEDBACK" identifier="HINT">
			<p> Use the quadratic formula</p>
		    <div>
		        <m:math display="block" id="mathML12">
		            <m:semantics>
		                <m:mtable>
		                    <m:mtr>
		                        <m:mtd columnalign="right">
		                            <m:mi>x</m:mi>
		                        </m:mtd>
		                        <m:mtd columnalign="right"/>
		                        <m:mtd columnalign="center">
		                            <m:mo>=</m:mo>
		                        </m:mtd>
		                        <m:mtd columnalign="left">
		                            <m:mfrac>
		                                <m:mrow>
		                                    <m:mo>-</m:mo>
		                                    <m:mi>b</m:mi>
		                                    <m:mo>&#xb1;</m:mo>
		                                    <m:msqrt>
		                                        <m:msup>
		                                            <m:mi>b</m:mi>
		                                            <m:mn>2</m:mn>
		                                        </m:msup>
		                                        <m:mo>-</m:mo>
		                                        <m:mfenced>
		                                            <m:mn>4</m:mn>
		                                            <m:mi>a</m:mi>
		                                            <m:mi>c</m:mi>
		                                        </m:mfenced>
		                                    </m:msqrt>
		                                </m:mrow>
		                                <m:mrow>
		                                    <m:mn>2</m:mn>
		                                    <m:mi>a</m:mi>
		                                </m:mrow>
		                            </m:mfrac>
		                        </m:mtd>
		                    </m:mtr>
		                </m:mtable>
		                <m:annotation encoding="SnuggleTeX">\[x = \frac{-b\plusmin \sqrt{b^2-4ac}{2a}}\]</m:annotation>
		            </m:semantics>
		        </m:math>
		    </div>
		</feedbackBlock>
		<feedbackBlock identifier="SEEN-SOLUTION" outcomeIdentifier="FEEDBACK" showHide="show">
			<p> Since you have viewed the solution, your score for this question will be 0. </p>
		</feedbackBlock>
		<feedbackBlock identifier="SEEN-HINT" outcomeIdentifier="FEEDBACK" showHide="show">
			<p> Since you have viewed the hint, your score for this question will be halved. </p>
		</feedbackBlock>
		<feedbackBlock identifier="askhint" outcomeIdentifier="ASKHINT" showHide="show">
			<p>
				<endAttemptInteraction id="endAttemptInteraction1" responseIdentifier="HINTREQUEST"
					title="Show Hint"/>
			</p>
		</feedbackBlock>
		<feedbackBlock identifier="asksolution" outcomeIdentifier="ASKSOLUTION" showHide="show">
			<p>
				<endAttemptInteraction id="endAttemptInteraction2" responseIdentifier="SOLREQUEST"
					title="Show Solution"/>
			</p>
		</feedbackBlock>
	</itemBody>
	<responseProcessing>
		<setOutcomeValue identifier="FEEDBACK">
			<multiple>
				<variable identifier="EMPTY"/>
			</multiple>
		</setOutcomeValue>
		<responseCondition>
			<responseIf>
				<variable identifier="HINTREQUEST"/>
				<setOutcomeValue identifier="FEEDBACK">
					<multiple>
						<baseValue baseType="identifier">HINT</baseValue>
					</multiple>
				</setOutcomeValue>
				<setOutcomeValue identifier="seenHint">
					<baseValue baseType="boolean">true</baseValue>
				</setOutcomeValue>
			</responseIf>
			<responseElseIf>
				<variable identifier="SOLREQUEST"/>
				<setOutcomeValue identifier="FEEDBACK">
					<multiple>
						<baseValue baseType="identifier">SOLUTION</baseValue>
					</multiple>
				</setOutcomeValue>
				<setOutcomeValue identifier="seenSolution">
					<baseValue baseType="boolean">true</baseValue>
				</setOutcomeValue>
				<setOutcomeValue identifier="completionStatus">
					<baseValue baseType="identifier">completed</baseValue>
				</setOutcomeValue>
				<setOutcomeValue identifier="ASKHINT">
					<baseValue baseType="identifier">null</baseValue>
				</setOutcomeValue>
				<setOutcomeValue identifier="ASKSOLUTION">
					<baseValue baseType="identifier">null</baseValue>
				</setOutcomeValue>
			</responseElseIf>
			<responseElse>
			    <responseCondition>
			        <responseIf>
			            <or>
			                <isNull>
			                    <variable identifier="RESPONSE1"/>
			                </isNull>
			                <isNull>
			                    <variable identifier="RESPONSE2"/>
			                </isNull>
			            </or>
			            <setOutcomeValue identifier="SCORE">
			                <baseValue baseType="float">0.0</baseValue>
			            </setOutcomeValue>
			        </responseIf>
			        <responseElse>
			            <responseCondition><!-- make sure larger value is in floatRespValue1 -->
			                <responseIf>
			                    <gte>
			                        <variable identifier="RESPONSE1"/>
			                        <variable identifier="RESPONSE2"/>
			                    </gte>
			                    <setOutcomeValue identifier="floatRespValue1">
			                        <variable identifier="RESPONSE1"/>
			                    </setOutcomeValue>
			                    <setOutcomeValue identifier="floatRespValue2">
			                        <variable identifier="RESPONSE2"/>
			                    </setOutcomeValue>
			                    <setOutcomeValue identifier="sResp1">
			                        <baseValue baseType="string">first</baseValue>
			                    </setOutcomeValue>
			                    <setOutcomeValue identifier="sResp2">
			                        <baseValue baseType="string">second</baseValue>
			                    </setOutcomeValue>
			                </responseIf>
			                <responseElse>
			                    <setOutcomeValue identifier="floatRespValue1">
			                        <variable identifier="RESPONSE2"/>
			                    </setOutcomeValue>
			                    <setOutcomeValue identifier="floatRespValue2">
			                        <variable identifier="RESPONSE1"/>
			                    </setOutcomeValue>
			                    <setOutcomeValue identifier="sResp1">
