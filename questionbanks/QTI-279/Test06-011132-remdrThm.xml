    <templateProcessing>
        <setTemplateValue identifier="iC">
            <randomInteger min="2" max="5"/>
        </setTemplateValue>
        <setTemplateValue identifier="iD">
            <product>
                <randomInteger min="1" max="5"/>
                <randomInteger min="-1" max="1" step="2"/>
            </product>
        </setTemplateValue>
        <setTemplateValue identifier="iArray">
            <multiple>
                <baseValue baseType="integer">5</baseValue>
                <baseValue baseType="integer">4</baseValue>
                <baseValue baseType="integer">3</baseValue>
                <baseValue baseType="integer">2</baseValue>
                <baseValue baseType="integer">1</baseValue>
                <baseValue baseType="integer">-5</baseValue>
                <baseValue baseType="integer">-4</baseValue>
                <baseValue baseType="integer">-3</baseValue>
                <baseValue baseType="integer">-2</baseValue>
                <baseValue baseType="integer">-1</baseValue>
            </multiple>
        </setTemplateValue>
        <setTemplateValue identifier="iA">
            <random>
                <variable identifier="iArray"/>
            </random>
        </setTemplateValue>
        <setTemplateValue identifier="iArray">
            <delete>
                <variable identifier="iA"/>
                <variable identifier="iArray"/>
            </delete>
        </setTemplateValue>
        <setTemplateValue identifier="iB">
            <random>
                <variable identifier="iArray"/>
            </random>
        </setTemplateValue>
        <setTemplateValue identifier="iArray">
            <delete>
                <variable identifier="iB"/>
                <variable identifier="iArray"/>
            </delete>
        </setTemplateValue>
        <setTemplateValue identifier="iType">
            <randomInteger min="1" max="2"/>
        </setTemplateValue>
        <setTemplateValue identifier="iK">
            <index n="iType">
                <ordered>
                    <variable identifier="iA"/>
                    <random>
                        <variable identifier="iArray"/>
                    </random>
                </ordered>
            </index>
        </setTemplateValue>
        <setTemplateValue identifier="sFactor">
            <index n="iType">
                <ordered>
                    <baseValue baseType="string"> is </baseValue>
                    <baseValue baseType="string"> is not </baseValue>
                </ordered>
            </index>
        </setTemplateValue>
        <setTemplateValue identifier="tDummy">
            <customOperator class="org.qtitools.mathassess.ScriptRule" ma:syntax="text/x-maxima">
                <baseValue baseType="string"><![CDATA[

 simp:true;

 mValX: ev(iK,simp);

 iA1: ev(iC,simp);

 iA2: ev(-(iA*iC+iD+iB*iC),simp);

 iA3: ev(iA*iD+iA*iB*iC+iB*iD,simp);

 iA4: ev(-iA*iB*iD,simp);

 iB1: ev(iC,simp);

 iB2: ev(-(iD+iB*iC),simp);

 iB3: ev(iB*iD,simp);

 iB4: 0;

 mX: x;

 mY: y;

 mQues: ev(iA1*mX^3+iA2*mX^2+iA3*mX+iA4,simp);
 mQuad:  ev(iB1*mX^2+iB2*mX+iB3,simp);
 mFactor1: ev(mX-iK,simp);
 
 iT1: ev(iA1*iK^3,simp);
 iT2: ev(iA2*iK^2,simp);
 iT3: ev(iA3*iK,simp);
 if is(iT2>=0)=true then mSignT2:maOperator("+") else mSignT2:maOperator("-");
 iT2:abs(iT2);
 if is(iT3>=0)=true then mSignT3:maOperator("+") else mSignT3:maOperator("-");
 iT3:abs(iT3);
 
 iAbsA2:abs(iA2);
 iAbsA3:abs(iA3);
 iAbsA4:abs(iA4);
 if is(iA2>=0)=true then mSignA2:maOperator("+") else mSignA2:maOperator("-");
 if is(iA3>=0)=true then mSignA3:maOperator("+") else mSignA3:maOperator("-");
 if is(iA4>=0)=true then mSignA4:maOperator("+") else mSignA4:maOperator("-");

 iVal: ev(subst(iK,mX,mQues),simp);

 

]]></baseValue>
            </customOperator>
        </setTemplateValue>
        <setCorrectResponse identifier="RESPONSE1">
            <variable identifier="iVal"/>
        </setCorrectResponse>
        <setCorrectResponse identifier="RESPONSE2">
            <index n="iType">
                <ordered>
                    <baseValue baseType="identifier">Choice1</baseValue>
                    <baseValue baseType="identifier">Choice2</baseValue>
                </ordered>
            </index>
        </setCorrectResponse>
    </templateProcessing>
    <itemBody class="">
        <div class="">
            <p>Find the remainder when
                <m:math id="mathML0">
                    <m:semantics>
                        <m:mrow>
                            <m:mi>f</m:mi>
                            <m:mfenced close=")" open="(">
                                <m:mi>mX</m:mi>
                            </m:mfenced>
                            <m:mo>=</m:mo>
                            <m:mi>mQues</m:mi>
                        </m:mrow>
                        <m:annotation encoding="SnuggleTeX">\[f(\qv{mX}) =
                            \qv{mQues}\]</m:annotation>
                    </m:semantics>
                </m:math>
            &#160;is divided by&#160;(<printedVariable identifier="mFactor1"/>), and complete the statement below:</p>
            <div class="">
                <table class="">
                    <tbody class="">
                        <tr class="">
                            <td class="">
                                Remainder =&#160;
                            </td>
                            <td class="">
                                <textEntryInteraction responseIdentifier="RESPONSE1" expectedLength="20"/>
                            </td>
                            <td class="">
                                <div class="">
                                    <feedbackInline id="feedbackInline2" identifier="INCORRECT1"
                                        outcomeIdentifier="FEEDBACK" showHide="show"> Your value for the remainder is incorrect. </feedbackInline>
                                </div>
                                <div class="">
                                    <feedbackInline id="feedbackInline2" identifier="SIGN"
                                        outcomeIdentifier="FEEDBACK" showHide="show"> No, the value you entered for the remainder has the wrong sign. </feedbackInline>
                                </div>
                                <div class="">
                                    <feedbackInline id="feedbackInline3" identifier="CORRECT1"
                                        outcomeIdentifier="FEEDBACK" showHide="show"> Well done, that's the correct value for the remainder.
                                    </feedbackInline>
                                </div>
                            </td>
                        </tr>
                        <tr class="">
                            <td colspan="2">
                                So&#160;(<printedVariable identifier="mFactor1"/>)&#160;
                                <inlineChoiceInteraction responseIdentifier="RESPONSE2" shuffle="false">
                                    <inlineChoice identifier="Choice1">is</inlineChoice>
                                    <inlineChoice identifier="Choice2">is not</inlineChoice>
                                </inlineChoiceInteraction>&#160;a factor of&#160;<printedVariable identifier="mQues"/>.
                            </td>
                            <td class="">
                                <div class="">
                                    <feedbackInline id="feedbackInline2" identifier="INCORRECT2"
                                        outcomeIdentifier="FEEDBACK" showHide="show"> Incorrect -
                                        please see the solution. </feedbackInline>
                                </div>
                                <div class="">
                                    <feedbackInline id="feedbackInline3" identifier="CORRECT2"
                                        outcomeIdentifier="FEEDBACK" showHide="show"> Well done, that's the correct conclusion.
                                    </feedbackInline>
                                </div>
                                <div class="">
                                    <feedbackInline id="feedbackInline3" identifier="NullInput"
                                        outcomeIdentifier="FEEDBACK" showHide="show"> Please enter a value in the box and select an option, then submit your answer
                                    </feedbackInline>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="">
                <feedbackBlock id="feedbackBlock4" identifier="THREETRIES"
                    outcomeIdentifier="FEEDBACK" showHide="show">
                    <p> You have had three attempts at answering this question, so here is the
                        solution: </p>
                </feedbackBlock>
                <feedbackBlock class="" id="feedbackBlock5" identifier="SOLUTION"
                    outcomeIdentifier="FEEDBACK" showHide="show">
                    <div class="">
                        <p>By the Remainder Theorem, the remainder when
                            <m:math id="mathML06">
                                <m:semantics>
                                    <m:mrow>
                                        <m:mi>f</m:mi>
                                        <m:mfenced close=")" open="(">
                                            <m:mi>mX</m:mi>
                                        </m:mfenced>
                                        <m:mo>=</m:mo>
                                        <m:mi>mQues</m:mi>
                                    </m:mrow>
                                    <m:annotation encoding="SnuggleTeX">\[f(\qv{mX}) =
                                        \qv{mQues}\]</m:annotation>
                                </m:semantics>
                            </m:math>
                            &#160;is divided by&#160;(<printedVariable identifier="mFactor1"/>)&#160;
                            is found by substituting for&#160;<m:math id="mathML07">
                                <m:semantics>
                                    <m:mrow>
                                        <m:mi>mX</m:mi>
                                        <m:mo>=</m:mo>
                                        <m:mi>mValX</m:mi>
                                    </m:mrow>
                                    <m:annotation encoding="SnuggleTeX">\[\qv{mX} =
                                        \qv{mValX}\]</m:annotation>
                                </m:semantics>
                            </m:math>&#160;in&#160;<m:math id="mathML05">
                                <m:semantics>
                                    <m:mrow>
                                        <m:mi>f</m:mi>
                                        <m:mfenced close=")" open="(">
                                            <m:mi>mX</m:mi>
                                        </m:mfenced>
                                    </m:mrow>
                                    <m:annotation encoding="SnuggleTeX">\[f(\qv{mX})\]</m:annotation>
                                </m:semantics>
                            </m:math>
                        </p>
                        <p>
                            <m:math display="block" id="mathML21">
                                <m:semantics>
                                    <m:mtable>
                                        <m:mtr>
                                            <m:mtd columnalign="right">
                                                <m:mi>f</m:mi>
                                                <m:mfenced close=")" open="(">
                                                    <m:mi>mValX</m:mi>
                                                </m:mfenced>
                                            </m:mtd>
                                            <m:mtd columnalign="center">
                                                <m:mo>=</m:mo>
                                            </m:mtd>
                                            <m:mtd columnalign="left">
                                                <m:mi>iA1</m:mi>
                                                <m:msup>
                                                    <m:mfenced open="(" close=")">
                                                        <m:mi>mValX</m:mi>
                                                    </m:mfenced>
                                                    <m:mn>3</m:mn>
                                                </m:msup>
                                                <m:mi>mSignA2</m:mi>
                                                <m:mi>iAbsA2</m:mi>
                                                <m:msup>
                                                    <m:mfenced open="(" close=")">
                                                        <m:mi>mValX</m:mi>
                                                    </m:mfenced>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                                <m:mi>mSignA3</m:mi>
                                                <m:mi>iAbsA3</m:mi>
                                                <m:mfenced open="(" close=")">
                                                    <m:mi>mValX</m:mi>
                                                </m:mfenced>
                                                <m:mi>mSignA4</m:mi>
                                                <m:mi>iAbsA4</m:mi>
                                            </m:mtd>
                                        </m:mtr>
                                        <m:mtr>
                                            <m:mtd columnalign="right">
                                                <m:mi>f</m:mi>
                                                <m:mfenced close=")" open="(">
                                                    <m:mi>mValX</m:mi>
                                                </m:mfenced>
                                            </m:mtd>
                                            <m:mtd columnalign="center">
                                                <m:mo>=</m:mo>
                                            </m:mtd>
                                            <m:mtd columnalign="left">
                                                <m:mi>iT1</m:mi>
                                                <m:mi>mSignT2</m:mi>
                                                <m:mi>iT2</m:mi>
                                                <m:mi>mSignT3</m:mi>
                                                <m:mi>iT3</m:mi>
                                                <m:mi>mSignA4</m:mi>
                                                <m:mi>iAbsA4</m:mi>
                                            </m:mtd>
                                        </m:mtr>
                                        <m:mtr>
                                            <m:mtd columnalign="right">
                                                <m:mi>f</m:mi>
                                                <m:mfenced close=")" open="(">
                                                    <m:mi>mValX</m:mi>
                                                </m:mfenced>
                                            </m:mtd>
                                            <m:mtd columnalign="center">
                                                <m:mo>=</m:mo>
                                            </m:mtd>
                                            <m:mtd columnalign="left">
                                                <m:mi>iVal</m:mi>
                                            </m:mtd>
                                        </m:mtr>
                                    </m:mtable>
                                    <m:annotation encoding="SnuggleTeX">\begin{eqnarray*}
                                        f(\qv{mValX})&amp;=&amp;qv{iA1}(\qv{mValX})^3\qv{mSignA2}\qv{iAbsA2}(\qv{mValX})^2\qv{mSignA3}\qv{iAbsA3}(\qv{mValX})\qv{mSignA4}\qv{iAbsA4} \\
                                        f(\qv{mValX})&amp;=&amp; 0\qv{iT1}\qv{mSignT2}\qv{iT2}\qv{mSignT3}\qv{iT3}\qv{mSignA4}\qv{iAbsA4} \\
                                        f(\qv{mValX})&amp;=&amp; \qv{iVal}
                                        \end{eqnarray*}</m:annotation>
                                </m:semantics>
                            </m:math>
                        </p>
                        <p>Hence&#160;<printedVariable identifier="mFactor1"/>&#160;<printedVariable identifier="sFactor"/>&#160;a factor of&#160;<printedVariable identifier="mQues"/>.</p>
                    </div>
                </feedbackBlock>
            </div>
            <div class="">
                <feedbackBlock id="feedbackBlock6" identifier="HINT" outcomeIdentifier="FEEDBACK"
                    showHide="show">
                    <p>Substitute for&#160;<m:math id="mathML04">
                        <m:semantics>
                            <m:mrow>
                                <m:mi>mX</m:mi>
                                <m:mo>=</m:mo>
                                <m:mi>mValX</m:mi>
                            </m:mrow>
                            <m:annotation encoding="SnuggleTeX">\[\qv{mX} =
                                \qv{mValX}\]</m:annotation>
                        </m:semantics>
                    </m:math>&#160;in&#160;<m:math id="mathML08">
                        <m:semantics>
                            <m:mrow>
                                <m:mi>f</m:mi>
                                <m:mfenced close=")" open="(">
                                    <m:mi>mX</m:mi>
                                </m:mfenced>
                            </m:mrow>
                            <m:annotation encoding="SnuggleTeX">\[f(\qv{mX})\]</m:annotation>
                        </m:semantics>
                    </m:math>
                    </p>
                    <p>If (<printedVariable identifier="mFactor1"/>)&#160;is a factor of <printedVariable identifier="mQues"/>, the result is equal to zero.</p>
                </feedbackBlock>
            </div>
            <feedbackBlock id="feedbackBlock7" identifier="SEEN-SOLUTION"
                outcomeIdentifier="FEEDBACK" showHide="show">
                <p> Since you have viewed the solution, your score for this question will be 0. </p>
            </feedbackBlock>
            <feedbackBlock id="feedbackBlock8" identifier="SEEN-HINT" outcomeIdentifier="FEEDBACK"
                showHide="show">
                <p> Since you have viewed the hint, your score for this question will be halved.
                </p>
            </feedbackBlock>
            <feedbackBlock id="feedbackBlock9" identifier="askhint" outcomeIdentifier="ASKHINT"
                showHide="show">
                <p>
                    <endAttemptInteraction id="endAttemptInteraction1"
                        responseIdentifier="HINTREQUEST" title="Show Hint"/>
                </p>
            </feedbackBlock>
            <feedbackBlock id="feedbackBlock10" identifier="asksolution"
                outcomeIdentifier="ASKSOLUTION" showHide="show">
                <p>
                    <endAttemptInteraction id="endAttemptInteraction2"
                        responseIdentifier="SOLREQUEST" title="Show Solution"/>
                </p>
            </feedbackBlock>
        </div>
    </itemBody>
    <responseProcessing>
        <setOutcomeValue identifier="FEEDBACK">
            <multiple>
                <variable identifier="EMPTY"/>
            </multiple>
        </setOutcomeValue>
        <responseCondition>
            <responseIf>
                <variable identifier="HINTREQUEST"/>
                <setOutcomeValue identifier="FEEDBACK">
                    <multiple>
                        <baseValue baseType="identifier">HINT</baseValue>
                    </multiple>
                </setOutcomeValue>
                <setOutcomeValue identifier="seenHint">
                    <baseValue baseType="boolean">true</baseValue>
                </setOutcomeValue>
            </responseIf>
            <responseElseIf>
                <variable identifier="SOLREQUEST"/>
                <setOutcomeValue identifier="FEEDBACK">
                    <multiple>
                        <baseValue baseType="identifier">SOLUTION</baseValue>
                    </multiple>
                </setOutcomeValue>
                <setOutcomeValue identifier="seenSolution">
                    <baseValue baseType="boolean">true</baseValue>
                </setOutcomeValue>
                <setOutcomeValue identifier="completionStatus">
                    <baseValue baseType="identifier">completed</baseValue>
                </setOutcomeValue>
                <setOutcomeValue identifier="ASKHINT">
                    <baseValue baseType="identifier">null</baseValue>
                </setOutcomeValue>
                <setOutcomeValue identifier="ASKSOLUTION">
                    <baseValue baseType="identifier">null</baseValue>
                </setOutcomeValue>
            </responseElseIf>
            <responseElse>
                <responseCondition>
                    <responseIf>
                        <or>
                            <isNull>
                                <variable identifier="RESPONSE1"/>
                            </isNull>
                            <isNull>
                                <variable identifier="RESPONSE2"/>
                            </isNull>
                        </or>
                        <setOutcomeValue identifier="FEEDBACK">
                            <multiple>
                                <baseValue baseType="identifier">NullInput</baseValue>
                            </multiple>
                        </setOutcomeValue>
                        <setOutcomeValue identifier="SCORE">
                            <baseValue baseType="float">0.0</baseValue>
                        </setOutcomeValue>
                    </responseIf>
                    <responseElse>
                        <responseCondition>
                            <responseIf>
                                <equal toleranceMode="exact">
                                    <variable identifier="RESPONSE1"/>
                                    <correct identifier="RESPONSE1"/>
                                </equal>
                                <setOutcomeValue identifier="FEEDBACK">
                                    <multiple>
