                                                </m:msup>
                                                <m:msup>
                                                    <m:mi>j</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                            </m:mtd>
                                            <m:mtd columnalign="center">
                                                <m:mo>=</m:mo>
                                            </m:mtd>
                                            <m:mtd columnalign="left">
                                                <m:mn>0</m:mn>
                                            </m:mtd>
                                        </m:mtr>
                                        <m:mtr>
                                            <m:mtd columnalign="right">
                                                <m:msup>
                                                    <m:mi>z</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                                <m:mi>signa</m:mi>
                                                <m:mn>2</m:mn>
                                                <m:mo>&#xd7;</m:mo>
                                                <m:mi>absa</m:mi>
                                                <m:mi>z</m:mi>
