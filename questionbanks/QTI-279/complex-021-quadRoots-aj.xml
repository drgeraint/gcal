                                                    </mrow>
                                                </m:mfenced>
                                                <m:mfenced close=")" open="(">
                                                    <mrow>
                                                        <m:mi>z</m:mi>
                                                        <m:mi>msignb</m:mi>
                                                        <m:mi>absb</m:mi>
                                                        <m:mi>j</m:mi>
                                                    </mrow>
                                                </m:mfenced>
                                            </m:mtd>
                                            <m:mtd columnalign="center">
                                                <m:mo>=</m:mo>
                                            </m:mtd>
                                            <m:mtd columnalign="left">
                                                <m:mn>0</m:mn>
                                            </m:mtd>
                                        </m:mtr>
                                        <m:mtr>
                                            <m:mtd columnalign="right">
                                                <m:msup>
                                                    <m:mi>z</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                                <m:mi>signb</m:mi>
                                                <m:mi>absb</m:mi>
                                                <m:mi>j</m:mi>
                                                <m:mi>z</m:mi>
                                                <m:mi>signb</m:mi>
                                                <m:mi>absb</m:mi>
                                                <m:mi>j</m:mi>
                                                <m:mi>z</m:mi>
                                                <m:mo>-</m:mo>
                                                <m:msup>
                                                    <m:mi>absb</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                                <m:msup>
                                                    <m:mi>j</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                            </m:mtd>
                                            <m:mtd columnalign="center">
                                                <m:mo>=</m:mo>
                                            </m:mtd>
                                            <m:mtd columnalign="left">
                                                <m:mn>0</m:mn>
                                            </m:mtd>
                                        </m:mtr>
                                        <m:mtr>
                                            <m:mtd columnalign="right">
                                                <m:msup>
                                                    <m:mi>z</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                                <m:mo>+</m:mo>
                                                <m:msup>
                                                    <m:mi>absb</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                            </m:mtd>
                                            <m:mtd columnalign="center">
                                                <m:mo>=</m:mo>
                                            </m:mtd>
                                            <m:mtd columnalign="left">
                                                <m:mn>0</m:mn>
                                            </m:mtd>
                                        </m:mtr>
                                        <m:mtr>
                                            <m:mtd columnalign="right">
                                                <m:msup>
                                                    <m:mi>z</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                                <m:mo>+</m:mo>
                                                <m:mi>absd</m:mi>
                                            </m:mtd>
                                            <m:mtd columnalign="center">
                                                <m:mo>=</m:mo>
                                            </m:mtd>
                                            <m:mtd columnalign="left">
                                                <m:mn>0</m:mn>
                                            </m:mtd>
                                        </m:mtr>
                                    </m:mtable>
                                    <m:annotation encoding="SnuggleTeX">\begin{eqnarray*}(z\qv{signb}\qv{absb}j)(z\qv{msignb}\qv{absb}j)&amp;=&amp;0 \\ z^2 \qv{signb}\qv{absb}jz \qv{signb}\qv{absb}jz -\qv{absb}^2 j^2 &amp;=&amp;0 \\z^2 +\qv{absb}^2 &amp;=&amp;0 \\z^2 +\qv{absd}&amp;=&amp;0 \end{eqnarray*}</m:annotation>
                                </semantics>
                            </math>
                        </templateInline>
                        <templateInline templateIdentifier="tExpr3" identifier="Sol2" showHide="show">
                            <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
