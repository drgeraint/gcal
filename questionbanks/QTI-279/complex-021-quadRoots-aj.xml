                                                <m:mn>0</m:mn>
                                            </m:mtd>
                                        </m:mtr>
                                        <m:mtr>
                                            <m:mtd columnalign="right">
                                                <m:msup>
                                                    <m:mi>z</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                                <m:mi>signa</m:mi>
                                                <m:mi>absa</m:mi>
                                                <m:mi>z</m:mi>
                                                <m:mi>signb</m:mi>
                                                <m:mi>absb</m:mi>
                                                <m:mi>j</m:mi>
                                                <m:mi>z</m:mi>
                                                <m:mi>signa</m:mi>
                                                <m:mi>absa</m:mi>
                                                <m:mi>z</m:mi>
                                                <m:mi>msignb</m:mi>
                                                <m:mi>absb</m:mi>
                                                <m:mi>j</m:mi>
                                                <m:mi>z</m:mi>
                                                <m:mo>+</m:mo>
                                                <m:msup>
                                                    <m:mi>absa</m:mi>
                                                    <m:mn>2</m:mn>
                                                </m:msup>
                                                <m:mo>+</m:mo>
                                                <m:mi>absab</m:mi>
                                                <m:mi>j</m:mi>
                                                <m:mo>-</m:mo>
                                                <m:mi>absab</m:mi>
                                                <m:mi>j</m:mi>
                                                <m:mo>-</m:mo>
                                                <m:msup>
                                                    <m:mi>absb</m:mi>
