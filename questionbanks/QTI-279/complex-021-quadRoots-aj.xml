                                            <m:mtd columnalign="right">
                                                <m:mfenced close=")" open="(">
                                                    <mrow>
                                                        <m:mi>z</m:mi>
                                                        <m:mi>signa</m:mi>
                                                        <m:mi>absa</m:mi>
                                                        <m:mi>signb</m:mi>
                                                        <m:mi>absb</m:mi>
                                                        <m:mi>j</m:mi>
                                                    </mrow>
                                                </m:mfenced>
                                                <m:mfenced close=")" open="(">
                                                    <mrow>
                                                        <m:mi>z</m:mi>
                                                        <m:mi>signa</m:mi>
                                                        <m:mi>absa</m:mi>
                                                        <m:mi>msignb</m:mi>
                                                        <m:mi>absb</m:mi>
                                                        <m:mi>j</m:mi>
                                                    </mrow>
                                                </m:mfenced>
                                            </m:mtd>
                                            <m:mtd columnalign="center">
                                                <m:mo>=</m:mo>
                                            </m:mtd>
