<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<assessmentItem toolVersion="0.5.1" toolName="Mathqurate" title="Find 3rd side in a scalene triangle, given 2 angles and a side"
    timeDependent="false" identifier="SineRule-003-3rdSideUnits" adaptive="true"
    xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqti_v2p1 imsqti_v2p1.xsd"
    xmlns="http://www.imsglobal.org/xsd/imsqti_v2p1" xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:m="http://www.w3.org/1998/Math/MathML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <responseDeclaration identifier="RESPONSE1" cardinality="single" baseType="float"/>
    <responseDeclaration identifier="RESPONSE2" cardinality="single" baseType="string"/>
    <responseDeclaration identifier="HINTREQUEST" cardinality="single" baseType="boolean"/>
    <responseDeclaration identifier="SOLREQUEST" cardinality="single" baseType="boolean"/>
    <outcomeDeclaration baseType="float" cardinality="single" identifier="SCORE"
        normalMaximum="3.0" normalMinimum="0.0">
        <defaultValue>
            <value>0.0</value>
        </defaultValue>
    </outcomeDeclaration>
    <outcomeDeclaration baseType="identifier" cardinality="multiple" identifier="FEEDBACK"/>
    <outcomeDeclaration baseType="identifier" cardinality="single" identifier="EMPTY"/>
    <outcomeDeclaration baseType="boolean" cardinality="single" identifier="seenSolution">
        <defaultValue>
            <value>false</value>
        </defaultValue>
    </outcomeDeclaration>
    <outcomeDeclaration baseType="boolean" cardinality="single" identifier="seenHint">
        <defaultValue>
            <value>false</value>
        </defaultValue>
    </outcomeDeclaration>
    <outcomeDeclaration baseType="identifier" cardinality="single" identifier="ASKHINT">
        <defaultValue>
            <value>askhint</value>
        </defaultValue>
    </outcomeDeclaration>
    <outcomeDeclaration baseType="identifier" cardinality="single" identifier="ASKSOLUTION">
        <defaultValue>
            <value>asksolution</value>
        </defaultValue>
    </outcomeDeclaration>
    <outcomeDeclaration identifier="oMult" cardinality="single" baseType="float">
        <defaultValue>
            <value>1.0</value>
        </defaultValue>
    </outcomeDeclaration>
    <outcomeDeclaration identifier="oPower" cardinality="single" baseType="float"/>
    <outcomeDeclaration identifier="unitsOK" cardinality="single" baseType="boolean">
        <defaultValue>
            <value>true</value>
        </defaultValue>
    </outcomeDeclaration>
    <templateDeclaration identifier="iA" cardinality="single" baseType="integer"
        paramVariable="false" mathVariable="true"/>
    <templateDeclaration identifier="iB" cardinality="single" baseType="integer"
        paramVariable="false" mathVariable="true"/>
    <templateDeclaration identifier="iC" cardinality="single" baseType="integer"
        paramVariable="false" mathVariable="true"/>
    <templateDeclaration identifier="ia" cardinality="single" baseType="integer"
        paramVariable="false" mathVariable="true"/>
    <templateDeclaration identifier="iUnits" cardinality="single" baseType="integer"
        paramVariable="false" mathVariable="true"/>
    <templateDeclaration identifier="sinA" cardinality="single" baseType="float"
        paramVariable="false" mathVariable="true"/>
    <templateDeclaration identifier="sinC" cardinality="single" baseType="float"
        paramVariable="false" mathVariable="true"/>
    <templateDeclaration identifier="fAns" cardinality="single" baseType="float"
        paramVariable="false" mathVariable="true"/>
    <templateDeclaration identifier="fPiOver180" cardinality="single" baseType="float"
        paramVariable="false" mathVariable="true"/>
    <templateDeclaration identifier="sUnits" cardinality="single" baseType="string"
        paramVariable="false" mathVariable="false"/>
    <templateProcessing>
        <!-- now do the randomisation and mathematical calculations -->
        <setTemplateValue identifier="iA">
            <randomInteger min="5" max="100"/>
        </setTemplateValue>
        <setTemplateValue identifier="iB">
            <randomInteger min="5" max="70"/>
        </setTemplateValue>
        <setTemplateValue identifier="ia">
            <randomInteger min="5" max="50"/>
        </setTemplateValue>
        <setTemplateValue identifier="iUnits">
            <randomInteger min="1" max="2"/>
        </setTemplateValue>
        <templateCondition>
            <templateIf>
                <equal toleranceMode="exact">
                    <variable identifier="iUnits"/>
                    <baseValue baseType="integer">2</baseValue>
                </equal>
                <setTemplateValue identifier="sUnits">
                    <baseValue baseType="string">m</baseValue>
                </setTemplateValue>
            </templateIf>
            <templateElse>
                <setTemplateValue identifier="sUnits">
                    <baseValue baseType="string">cm</baseValue>
                </setTemplateValue>
            </templateElse>
        </templateCondition>
        <setTemplateValue identifier="iC">
            <subtract>
                <baseValue baseType="integer">180</baseValue>
                <sum>
                    <variable identifier="iA"/>
                    <variable identifier="iB"/>
                </sum>
            </subtract>
        </setTemplateValue>
        <setTemplateValue identifier="fPiOver180">
            <divide>
                <mathConstant name="pi"/>
                <baseValue baseType="integer">180</baseValue>
            </divide>
        </setTemplateValue>
        <setTemplateValue identifier="sinA">
            <mathOperator name="sin">
                <product>
                    <variable identifier="iA"/>
                    <variable identifier="fPiOver180"/>
                </product>
            </mathOperator>
        </setTemplateValue>
        <setTemplateValue identifier="sinC">
            <mathOperator name="sin">
                <product>
                    <variable identifier="iC"/>
                    <variable identifier="fPiOver180"/>
                </product>
            </mathOperator>
        </setTemplateValue>
        <setTemplateValue identifier="sinA">
            <roundTo roundingMode="decimalPlaces" figures="5">
                <variable identifier="sinA"/>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="sinC">
            <roundTo roundingMode="decimalPlaces" figures="5">
                <variable identifier="sinC"/>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="fAns">
            <divide>
                <product>
                    <variable identifier="ia"/>
                    <variable identifier="sinC"/>
                </product>
                <variable identifier="sinA"/>
            </divide>
        </setTemplateValue>
        <setTemplateValue identifier="fAns">
            <roundTo roundingMode="decimalPlaces" figures="2">
                <variable identifier="fAns"/>
            </roundTo>
        </setTemplateValue>
    </templateProcessing>
    <itemBody>
        <p>In triangle ABC, side a=<printedVariable identifier="ia"/><printedVariable
                identifier="sUnits"/>, angle A=<printedVariable identifier="iA"/>&#xb0; and angle
                B=<printedVariable identifier="iB"/>&#xb0;. Find the length of side c.</p>
        <div class="">
            <p>Enter the numerical part of your answer to 2 decimal places in the first box,
                and the units in the second box (mm, cm, m or km may be used).</p>
            <textEntryInteraction responseIdentifier="RESPONSE1" expectedLength="20"/>
            <textEntryInteraction responseIdentifier="RESPONSE2" expectedLength="5"/>
        </div>
        <div class="">
            <feedbackInline id="feedbackInline3" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Correct"> OK </feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Partial"> Not far off... </feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Partial"> You should have multiplied by an integer power of 10 </feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Incorrect"> No, but you have the units correct. </feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="OKNoUnits"> OK, but your value for the units is incorrect</feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="PartialNoUnits"> Not far off, but your value for the units is incorrect
            </feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Power10NoUnits"> You should have multiplied by an integer power of 10, and your value for the units is incorrect
            </feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Missing"> Please use both input boxes
            </feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="IncorrectNoUnits"> Incorrect, and the units are incorrect
            </feedbackInline></div>
        <div class="">
            <feedbackBlock id="feedbackBlock11" identifier="THREETRIES" outcomeIdentifier="FEEDBACK"
                showHide="show">
                <p> You have had three attempts at answering this question, so here is the solution:
                </p>
            </feedbackBlock>
            <feedbackBlock identifier="SOLUTION" outcomeIdentifier="FEEDBACK" showHide="show">
                <p>We apply the sine rule</p>
                <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
                    <semantics>
                        <mtable>
                            <mtr>
                                <mtd columnalign="center">
                                    <mfrac>
                                        <mi>a</mi>
                                        <mrow>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>A</mi>
                                            </mfenced>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                                <mtd columnalign="center">
                                    <mo>=</mo>
                                </mtd>
                                <mtd columnalign="center">
                                    <mfrac>
                                        <mi>b</mi>
                                        <mrow>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>B</mi>
                                            </mfenced>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                                <mtd columnalign="center">
                                    <mo>=</mo>
                                </mtd>
                                <mtd columnalign="center">
                                    <mfrac>
                                        <mi>c</mi>
                                        <mrow>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>C</mi>
                                            </mfenced>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                            </mtr>
                            <mtr>
                                <mtd columnalign="center">
                                    <mfrac>
                                        <mi>ia</mi>
                                        <mrow>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>iA</mi>
                                            </mfenced>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                                <mtd columnalign="center">
                                    <mo>=</mo>
                                </mtd>
                                <mtd columnalign="center">
                                    <mfrac>
                                        <mi>b</mi>
                                        <mrow>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>iB</mi>
                                            </mfenced>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                                <mtd columnalign="center">
                                    <mo>=</mo>
                                </mtd>
                                <mtd columnalign="center">
                                    <mfrac>
                                        <mi>c</mi>
                                        <mrow>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>C</mi>
                                            </mfenced>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                            </mtr>
                        </mtable>
                        <annotation encoding="SnuggleTeX">\[ \begin{array}{ccccc}
                            \frac{a}{\sin(A)}&amp;=&amp;\frac{b}{\sin(B)}&amp;=&amp;\frac{c}{\sin(C)}\\
                            \frac{\qv{ia}}{\sin(\qv{iA})}&amp;=&amp;\frac{b}{\sin(\qv{iB})}&amp;=&amp;\frac{c}{\sin(C)}
                            \end{array} \]</annotation>
                    </semantics>
                </math>


                <p>Before we can find side c, we need to find the value of angle C:</p>
                <p><math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
                        <semantics>
                            <mtable>
                                <mtr>
                                    <mtd columnalign="right">
                                        <mi>A</mi>
                                        <mo>+</mo>
                                        <mi>B</mi>
                                        <mo>+</mo>
                                        <mi>C</mi>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <mn>180</mn>
                                    </mtd>
                                </mtr>
                                <mtr>
                                    <mtd columnalign="right">
                                        <mi>C</mi>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <mn>180</mn>
                                        <mo>-</mo>
                                        <mi>iA</mi>
                                        <mo>-</mo>
                                        <mi>iB</mi>
                                    </mtd>
                                </mtr>
                                <mtr>
                                    <mtd columnalign="right">
                                        <mi>C</mi>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <mi>iC</mi>
                                    </mtd>
                                </mtr>
                            </mtable>
                            <annotation encoding="SnuggleTeX">\begin{eqnarray*}
                                A+B+C&amp;=&amp;180\\ C&amp;=&amp;180-\qv{iA}-\qv{iB}\\
                                C&amp;=&amp;\qv{iC} \end{eqnarray*}</annotation>
                        </semantics>
                    </math>
                </p>


                <p>Now we use the values of a, A and C in the sine rule equation to get c:</p>
                <p>
                <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
                    <semantics>
                        <mtable>
                            <mtr>
                                <mtd columnalign="right">
                                    <mfrac>
                                        <mi>ia</mi>
                                        <mrow>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>iA</mi>
                                            </mfenced>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                                <mtd columnalign="center">
                                    <mo>=</mo>
                                </mtd>
                                <mtd columnalign="left">
                                    <mfrac>
                                        <mi>c</mi>
                                        <mrow>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>iC</mi>
                                            </mfenced>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                            </mtr>
                            <mtr>
                                <mtd columnalign="right">
                                    <mi>c</mi>
                                </mtd>
                                <mtd columnalign="center">
                                    <mo>=</mo>
                                </mtd>
                                <mtd columnalign="left">
                                    <mfrac>
                                        <mrow>
                                            <mi>ia</mi>
                                            <mo>&#xd7;</mo>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>iC</mi>
                                            </mfenced>
                                        </mrow>
                                        <mrow>
                                            <mi>sin</mi>
                                            <mfenced close=")" open="(">
                                                <mi>iA</mi>
                                            </mfenced>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                            </mtr>
                            <mtr>
                                <mtd columnalign="right"/>
                                <mtd columnalign="center">
                                    <mo>=</mo>
                                </mtd>
                                <mtd columnalign="left">
                                    <mfrac>
                                        <mrow>
                                            <mi>ia</mi>
                                            <mo>&#xd7;</mo>
                                            <mi>sinC</mi>
                                        </mrow>
                                        <mrow>
                                            <mi>sinA</mi>
                                        </mrow>
                                    </mfrac>
                                </mtd>
                            </mtr>
                            <mtr>
                                <mtd columnalign="right"/>
                                <mtd columnalign="center">
                                    <mo>=</mo>
                                </mtd>
                                <mtd columnalign="left">
                                    <mi>fAns</mi>
                                </mtd>
                            </mtr>
                        </mtable>
                        <annotation encoding="SnuggleTeX"
                            >\begin{eqnarray*}\frac{\qv{ia}}{\sin(\qv{iA})}&amp;=&amp;\frac{c}{\sin(\qv{iC})}\\
                            c&amp;=&amp;\frac{A\times\sin(C)}{\sin(A)}\\
                            &amp;=&amp;\frac{\qv{ia}\times\sin(\qv{iC})}{\sin(\qv{iA})}\\
                            &amp;=&amp;\qv{fAns} \end{eqnarray*}</annotation>
                    </semantics>
                </math>where this value is in <printedVariable identifier="sUnits"/></p>

            </feedbackBlock></div>
        <div class="">
            <feedbackBlock identifier="HINT" outcomeIdentifier="FEEDBACK" showHide="show">
                <p>Use the sine rule:</p>
                <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
                    <semantics>
                        <mrow>
                            <mfrac>
                                <mi>a</mi>
                                <mrow>
                                    <mi>sin</mi>
                                    <mfenced close=")" open="(">
                                        <mi>A</mi>
                                    </mfenced>
                                </mrow>
                            </mfrac>
                            <mo>=</mo>
                            <mfrac>
                                <mi>b</mi>
                                <mrow>
                                    <mi>sin</mi>
                                    <mfenced close=")" open="(">
                                        <mi>B</mi>
                                    </mfenced>
                                </mrow>
                            </mfrac>
                            <mo>=</mo>
                            <mfrac>
                                <mi>c</mi>
                                <mrow>
                                    <mi>sin</mi>
                                    <mfenced close=")" open="(">
                                        <mi>C</mi>
                                    </mfenced>
                                </mrow>
                            </mfrac>
                        </mrow>
                        <annotation encoding="SnuggleTeX"
                            >\[\frac{a}{\sin(A)}=\frac{b}{\sin(B)}=\frac{c}{\sin(C)}\]</annotation>
                    </semantics>
                </math>

            </feedbackBlock></div>
        <feedbackBlock identifier="SEEN-SOLUTION" outcomeIdentifier="FEEDBACK" showHide="show">
            <p> Since you have viewed the solution, your score for this question will be 0. </p>
        </feedbackBlock>
        <feedbackBlock identifier="SEEN-HINT" outcomeIdentifier="FEEDBACK" showHide="show">
            <p> Since you have viewed the hint, your score for this question will be halved. </p>
        </feedbackBlock>
        <feedbackBlock identifier="askhint" outcomeIdentifier="ASKHINT" showHide="show">
            <div>
                <endAttemptInteraction id="endAttemptInteraction1" responseIdentifier="HINTREQUEST"
                    title="Show Hint"/>
            </div>
        </feedbackBlock>
        <feedbackBlock identifier="asksolution" outcomeIdentifier="ASKSOLUTION" showHide="show">
            <div>
                <endAttemptInteraction id="endAttemptInteraction2" responseIdentifier="SOLREQUEST"
                    title="Show Solution"/>
            </div>
        </feedbackBlock>
    </itemBody>
    <responseProcessing>
        <setOutcomeValue identifier="FEEDBACK">
            <multiple>
                <variable identifier="EMPTY"/>
            </multiple>
        </setOutcomeValue>
        <responseCondition>
            <responseIf>
                <variable identifier="HINTREQUEST"/>
                <setOutcomeValue identifier="FEEDBACK">
                    <multiple>
                        <baseValue baseType="identifier">HINT</baseValue>
                    </multiple>
                </setOutcomeValue>
                <setOutcomeValue identifier="seenHint">
                    <baseValue baseType="boolean">true</baseValue>
                </setOutcomeValue>
            </responseIf>
            <responseElseIf>
                <variable identifier="SOLREQUEST"/>
                <setOutcomeValue identifier="FEEDBACK">
                    <multiple>
                        <baseValue baseType="identifier">SOLUTION</baseValue>
                    </multiple>
                </setOutcomeValue>
                <setOutcomeValue identifier="seenSolution">
                    <baseValue baseType="boolean">true</baseValue>
                </setOutcomeValue>
                <setOutcomeValue identifier="completionStatus">
                    <baseValue baseType="identifier">completed</baseValue>
                </setOutcomeValue>
                <setOutcomeValue identifier="ASKHINT">
                    <baseValue baseType="identifier">null</baseValue>
                </setOutcomeValue>
                <setOutcomeValue identifier="ASKSOLUTION">
                    <baseValue baseType="identifier">null</baseValue>
                </setOutcomeValue>
            </responseElseIf>
            <responseElse>
                <responseCondition>
                    <responseIf>
                        <or>
                            <isNull>
                                <variable identifier="RESPONSE1"/>
                            </isNull>
                            <isNull>
                                <variable identifier="RESPONSE2"/>
                            </isNull>
                        </or>
                        <setOutcomeValue identifier="SCORE">
                            <baseValue baseType="float">0</baseValue>
                        </setOutcomeValue>
                    </responseIf>
                    <responseElse>
                        <responseCondition>
                            <responseIf>
                                <match>
                                    <variable identifier="sUnits"/>
                                    <variable identifier="RESPONSE2"/>
                                </match>
                                <setOutcomeValue identifier="oMult">
                                    <baseValue baseType="float">1.0</baseValue>
                                </setOutcomeValue>
                            </responseIf>
                            <responseElse>
                                <responseCondition>
                                    <responseIf>
                                        <match>
                                            <variable identifier="sUnits"/>
                                            <baseValue baseType="string">cm</baseValue>
                                        </match>
                                        <responseCondition>
                                            <responseIf>
                                                <match>
                                                    <variable identifier="RESPONSE2"/>
                                                    <baseValue baseType="string">mm</baseValue>
                                                </match>
                                                <setOutcomeValue identifier="oMult">
                                                    <baseValue baseType="float">10.0</baseValue>
                                                </setOutcomeValue>
                                            </responseIf>
                                            <responseElseIf>
                                                <match>
                                                    <variable identifier="RESPONSE2"/>
                                                    <baseValue baseType="string">m</baseValue>
                                                </match>
                                                <setOutcomeValue identifier="oMult">
                                                    <baseValue baseType="float">0.01</baseValue>
                                                </setOutcomeValue>
                                            </responseElseIf>
                                            <responseElseIf>
                                                <match>
                                                    <variable identifier="RESPONSE2"/>
                                                    <baseValue baseType="string">km</baseValue>
                                                </match>
                                                <setOutcomeValue identifier="oMult">
                                                    <baseValue baseType="float">0.00001</baseValue>
                                                </setOutcomeValue>
                                            </responseElseIf>
                                            <responseElse>
                                                <setOutcomeValue identifier="oMult">
                                                    <baseValue baseType="float">1.0</baseValue>
                                                </setOutcomeValue>
                                                <setOutcomeValue identifier="unitsOK">
                                                    <baseValue baseType="boolean">false</baseValue>
                                                </setOutcomeValue>
                                            </responseElse>
                                        </responseCondition>
                                    </responseIf>
                                    <responseElseIf>
                                        <match>
                                            <variable identifier="sUnits"/>
                                            <baseValue baseType="string">m</baseValue>
                                        </match>
                                        <responseCondition>
                                            <responseIf>
                                                <match>
                                                    <variable identifier="RESPONSE2"/>
                                                    <baseValue baseType="string">mm</baseValue>
                                                </match>
                                                <setOutcomeValue identifier="oMult">
                                                    <baseValue baseType="float">1000.0</baseValue>
                                                </setOutcomeValue>
                                            </responseIf>
                                            <responseElseIf>
                                                <match>
                                                    <variable identifier="RESPONSE2"/>
                                                    <baseValue baseType="string">cm</baseValue>
                                                </match>
                                                <setOutcomeValue identifier="oMult">
                                                    <baseValue baseType="float">100.0</baseValue>
                                                </setOutcomeValue>
                                            </responseElseIf>
                                            <responseElseIf>
                                                <match>
                                                    <variable identifier="RESPONSE2"/>
                                                    <baseValue baseType="string">km</baseValue>
                                                </match>
                                                <setOutcomeValue identifier="oMult">
                                                    <baseValue baseType="float">0.001</baseValue>
                                                </setOutcomeValue>
                                            </responseElseIf>
                                            <responseElse>
                                                <setOutcomeValue identifier="oMult">
                                                    <baseValue baseType="float">1.0</baseValue>
                                                </setOutcomeValue>
                                                <setOutcomeValue identifier="unitsOK">
                                                    <baseValue baseType="boolean">false</baseValue>
                                                </setOutcomeValue>
                                            </responseElse>
                                        </responseCondition>
                                    </responseElseIf>
                                    <responseElse>
                                        <setOutcomeValue identifier="oMult">
                                            <baseValue baseType="float">1.0</baseValue>
                                        </setOutcomeValue>
                                        <setOutcomeValue identifier="unitsOK">
                                            <baseValue baseType="boolean">false</baseValue>
                                        </setOutcomeValue>
                                    </responseElse>
                                </responseCondition>
                            </responseElse>
                        </responseCondition>
                        <setOutcomeValue identifier="oPower">
                            <mathOperator name="log">
                                <divide>
                                    <variable identifier="fAns"/>
                                    <variable identifier="RESPONSE1"/>
                                </divide>
                            </mathOperator>
                        </setOutcomeValue>
                        <responseCondition>
                            <responseIf>
                                <equalRounded roundingMode="decimalPlaces" figures="2">
                                    <product>
                                        <variable identifier="fAns"/>
                                        <variable identifier="oMult"/>
                                    </product>
                                    <variable identifier="RESPONSE1"/>
