                    </responseElse>
                </responseCondition>
                <responseCondition>
                    <responseIf>
                        <variable identifier="seenSolution"/>
                        <setOutcomeValue identifier="FEEDBACK">
                            <multiple>
                                <variable identifier="FEEDBACK"/>
                                <baseValue baseType="identifier">SEEN-SOLUTION</baseValue>
                            </multiple>
                        </setOutcomeValue>
                        <setOutcomeValue identifier="SCORE">
