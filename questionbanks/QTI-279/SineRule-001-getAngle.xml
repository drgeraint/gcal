    <templateProcessing>
        <!-- now do the randomisation and mathematical calculations
Test(Random(1,3,1)<3,tqaa:=Random(60,89.5,0.1),tqaa:=Random(90.5,105,0.1))  -  angle A
tqb:=Random(2,5,1)+Random(0.1,0.9,0.1)  -  side b
tqc:=Random(2,5,1)+Random(0.1,0.9,0.1)  -  side c
Test(tqc=tqb,GoTo(IconID@"retry"))

- use cosine rule to get side a
tqa:=SQRT(tqb*tqb+tqc*tqc-2*tqb*tqc*COS(tqaa))  -  side a
Test(tqa=tqb,GoTo(IconID@"retry"))
Test(tqa=tqc,GoTo(IconID@"retry"))

tqa:=Round(tqa,1)

tqi:=SIN((tqaa*Pi)/180)
tqj:=(tqb*tqi)/tqa
- check validity - can't have ABS(sin(x))>1
Test(ABS(tqj)>1,GoTo(IconID@"retry"))
tqd:=ASIN(tqj)*180/Pi

tqdd:=Round(tqd,0)

tqk:=Random(1,3,1)  - randomise angle known
tql:=Random(1,2,1)  - randomise angle required
tqm:=tqk+tql
Test(tqm>3,tqm:=tqm-3,tqm:=tqm)
tqp:="A B C"
tqq:="a b c"
tqpp:=GetWord(tqk,tqp) - name of known angle
tqqq:=GetWord(tqm,tqp) - name of unknown angle
tqrr:=GetWord(tqk,tqq) - name of side corresponding to known angle
tqss:=GetWord(tqm,tqq) - name of side corresponding to unknown angle

        -->
        <templateCondition>
            <templateIf>
                <lt>
                    <randomInteger max="3" min="1"/>
                    <baseValue baseType="integer">3</baseValue>
                </lt>
                <setTemplateValue identifier="fC">
                    <roundTo roundingMode="decimalPlaces" figures="1">
                        <randomFloat min="60.0" max="89.5"/>
                    </roundTo>
                </setTemplateValue>
            </templateIf>
            <templateElse>
                <setTemplateValue identifier="fC">
                    <roundTo roundingMode="decimalPlaces" figures="1">
                        <randomFloat min="90.5" max="105.0"/>
                    </roundTo>
                </setTemplateValue>
            </templateElse>
        </templateCondition>
        <setTemplateValue identifier="fa">
            <roundTo roundingMode="decimalPlaces" figures="1">
                <randomFloat min="2.0" max="6.0"/>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="fb">
            <roundTo roundingMode="decimalPlaces" figures="1">
                <randomFloat min="2.0" max="6.0"/>
            </roundTo>
        </setTemplateValue>
        <templateConstraint>
            <gt>
                <mathOperator name="abs">
                    <subtract>
                        <variable identifier="fa"/>
                        <variable identifier="fb"/>
                    </subtract>
                </mathOperator>
                <baseValue baseType="float">0.05</baseValue>
            </gt>
        </templateConstraint>
        <setTemplateValue identifier="faSq">
            <roundTo roundingMode="decimalPlaces" figures="2">
                <power>
                    <variable identifier="fa"/>
                    <baseValue baseType="integer">2</baseValue>
                </power>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="fbSq">
            <roundTo roundingMode="decimalPlaces" figures="2">
                <power>
                    <variable identifier="fb"/>
                    <baseValue baseType="integer">2</baseValue>
                </power>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="f2ab">
            <roundTo roundingMode="decimalPlaces" figures="2">
                <product>
                    <baseValue baseType="integer">2</baseValue>
                    <variable identifier="fa"/>
                    <variable identifier="fb"/>
                </product>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="fPiOver180">
            <divide>
                <mathConstant name="pi"/>
                <baseValue baseType="integer">180</baseValue>
            </divide>
        </setTemplateValue>
        <setTemplateValue identifier="cosC">
            <mathOperator name="cos">
                <product>
                    <variable identifier="fC"/>
                    <variable identifier="fPiOver180"/>
                </product>
            </mathOperator>
        </setTemplateValue>
        <templateConstraint>
            <lte>
                <mathOperator name="abs">
                    <variable identifier="cosC"/>
                </mathOperator>
                <baseValue baseType="integer">1</baseValue>
            </lte>
        </templateConstraint>
        <setTemplateValue identifier="cosC">
            <roundTo roundingMode="decimalPlaces" figures="6">
                <variable identifier="cosC"/>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="fcSq">
            <roundTo roundingMode="decimalPlaces" figures="6">
                <subtract>
                    <sum>
                        <variable identifier="faSq"/>
                        <variable identifier="fbSq"/>
                    </sum>
                    <product>
                        <variable identifier="f2ab"/>
                        <variable identifier="cosC"/>
                    </product>
                </subtract>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="fc">
            <roundTo roundingMode="decimalPlaces" figures="1">
                <power>
                    <variable identifier="fcSq"/>
                    <baseValue baseType="float">0.5</baseValue>
                </power>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="sinC">
            <roundTo roundingMode="decimalPlaces" figures="6">
                <mathOperator name="sin">
                    <product>
                        <variable identifier="fC"/>
                        <variable identifier="fPiOver180"/>
                    </product>
                </mathOperator>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="sinA">
            <roundTo roundingMode="decimalPlaces" figures="6">
                <divide>
                    <product>
                        <variable identifier="fa"/>
                        <variable identifier="sinC"/>
                    </product>
                    <variable identifier="fc"/>
                </divide>
            </roundTo>
        </setTemplateValue>
        <templateConstraint>
            <lte>
                <mathOperator name="abs">
                    <variable identifier="sinA"/>
                </mathOperator>
                <baseValue baseType="integer">1</baseValue>
            </lte>
        </templateConstraint>
        <setTemplateValue identifier="fArad">
            <mathOperator name="asin">
                <variable identifier="sinA"/>
            </mathOperator>
        </setTemplateValue>
        <setTemplateValue identifier="fA">
            <divide>
                <variable identifier="fArad"/>
                <variable identifier="fPiOver180"/>
            </divide>
        </setTemplateValue>
        <setTemplateValue identifier="fAns">
            <roundTo roundingMode="decimalPlaces" figures="2">
                <variable identifier="fA"/>
            </roundTo>
        </setTemplateValue>
        <setTemplateValue identifier="fArad2">
            <mathOperator name="asin">
                <divide>
                    <product>
                        <variable identifier="fa"/>
                        <mathOperator name="sin">
                            <variable identifier="fC"/>
                        </mathOperator>
                    </product>
                    <variable identifier="fc"/>
                </divide>
            </mathOperator>
        </setTemplateValue>
        <setTemplateValue identifier="iUnits">
            <randomInteger min="1" max="2"/>
        </setTemplateValue>
        <templateCondition>
            <templateIf>
                <equal toleranceMode="exact">
                    <variable identifier="iUnits"/>
                    <baseValue baseType="integer">2</baseValue>
                </equal>
                <setTemplateValue identifier="sUnits">
                    <baseValue baseType="string">m</baseValue>
                </setTemplateValue>
            </templateIf>
            <templateElse>
                <setTemplateValue identifier="sUnits">
                    <baseValue baseType="string">cm</baseValue>
                </setTemplateValue>
            </templateElse>
        </templateCondition>
        <setTemplateValue identifier="iKnown">
            <randomInteger min="1" max="3"/>
        </setTemplateValue>
        <setTemplateValue identifier="iDiff">
            <randomInteger min="0" max="1"/>
        </setTemplateValue>
        <setTemplateValue identifier="iReq">
            <sum>
                <integerModulus>
                    <sum>
                        <variable identifier="iKnown"/>
                        <variable identifier="iDiff"/>
                    </sum>
                    <baseValue baseType="integer">3</baseValue>
                </integerModulus>
                <baseValue baseType="integer">1</baseValue>
            </sum>
        </setTemplateValue>
        <setTemplateValue identifier="sSides">
            <ordered>
                <baseValue baseType="string">a</baseValue>
                <baseValue baseType="string">b</baseValue>
                <baseValue baseType="string">c</baseValue>
            </ordered>
        </setTemplateValue>
        <setTemplateValue identifier="sAngles">
            <ordered>
                <baseValue baseType="string">A</baseValue>
                <baseValue baseType="string">B</baseValue>
                <baseValue baseType="string">C</baseValue>
            </ordered>
        </setTemplateValue>
        <setTemplateValue identifier="sSidea">
            <index n="iKnown">
                <variable identifier="sSides"/>
            </index>
        </setTemplateValue>
        <setTemplateValue identifier="sSideb">
            <index n="iReq">
                <variable identifier="sSides"/>
            </index>
        </setTemplateValue>
        <setTemplateValue identifier="sAngleA">
            <index n="iKnown">
                <variable identifier="sAngles"/>
            </index>
        </setTemplateValue>
        <setTemplateValue identifier="sAngleB">
            <index n="iReq">
                <variable identifier="sAngles"/>
            </index>
        </setTemplateValue>
    </templateProcessing>
    <itemBody>
        <p>In triangle ABC, side <printedVariable identifier="sSidea"/> = <printedVariable identifier="fc"/><printedVariable
            identifier="sUnits"/>, angle <printedVariable identifier="sAngleA"/> = <printedVariable identifier="fC"/>&#xb0; and side
            <printedVariable identifier="sSideb"/> = <printedVariable identifier="fa"/><printedVariable
                identifier="sUnits"/>. Find angle <printedVariable identifier="sAngleB"/> in degrees.</p>
        <div class="">
            <p>Enter the numerical part of your answer to 2 decimal places.</p>
            <textEntryInteraction responseIdentifier="RESPONSE1" expectedLength="20"/>&#xb0;
        </div>
        <div class="">
            <feedbackInline id="feedbackInline3" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Correct"> OK </feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Partial"> Not far off... </feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Sin"> You have entered the value of sin(<printedVariable identifier="sAngleB"/>); you need to use the inverse sine function to find the angle.</feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Radians"> You seem to have your calculator set to use radians.</feedbackInline></div>
        <div class="">
            <feedbackInline id="feedbackInline2" showHide="show" outcomeIdentifier="FEEDBACK"
                identifier="Incorrect"> No, that's not correct. </feedbackInline></div>
        <div class="">
            <feedbackBlock id="feedbackBlock11" identifier="THREETRIES" outcomeIdentifier="FEEDBACK"
                showHide="show">
                <p> You have had three attempts at answering this question, so here is the solution:
                </p>
            </feedbackBlock>
            <feedbackBlock identifier="SOLUTION" outcomeIdentifier="FEEDBACK" showHide="show">
                <p>We apply the sine rule</p>
                <p>
                    <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
                        <semantics>
                            <mtable>
                                <mtr>
                                    <mtd columnalign="center">
                                        <mfrac>
                                            <mi>sSidea</mi>
                                            <mrow>
                                                <mi>sin</mi>
                                                <mfenced close=")" open="(">
                                                    <mi>sAngleA</mi>
                                                </mfenced>
                                            </mrow>
                                        </mfrac>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <mfrac>
                                            <mi>sSideb</mi>
                                            <mrow>
                                                <mi>sin</mi>
                                                <mfenced close=")" open="(">
                                                    <mi>sAngleB</mi>
                                                </mfenced>
                                            </mrow>
                                        </mfrac>
                                    </mtd>
                                </mtr>
                                <mtr>
                                    <mtd columnalign="right">
                                        <mfrac>
                                            <mi>fc</mi>
                                            <mrow>
                                                <mi>sin</mi>
                                                <mfenced close=")" open="(">
                                                    <mi>fC</mi>
                                                </mfenced>
                                            </mrow>
                                        </mfrac>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <mfrac>
                                            <mi>fa</mi>
                                            <mrow>
                                                <mi>sin</mi>
                                                <mfenced close=")" open="(">
                                                    <mi>sAngleB</mi>
                                                </mfenced>
                                            </mrow>
                                        </mfrac>
                                    </mtd>
                                </mtr>
                                <mtr>
                                    <mtd columnalign="right">
                                        <mi>sin</mi>
                                        <mfenced close=")" open="(">
                                            <mi>sAngleB</mi>
                                        </mfenced>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <mfrac>
                                            <mrow>
                                                <mi>fa</mi>
                                                <mo>&#xd7;</mo>
                                                <mi>sin</mi>
                                                <mfenced close=")" open="(">
                                                    <mi>fC</mi>
                                                </mfenced>
                                            </mrow>
                                            <mrow>
                                                <mi>fc</mi>
                                            </mrow>
                                        </mfrac>
                                    </mtd>
                                </mtr>
                                <mtr>
                                    <mtd columnalign="right">
                                        <mi>sin</mi>
                                        <mfenced close=")" open="(">
                                            <mi>sAngleB</mi>
                                        </mfenced>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <mfrac>
                                            <mrow>
                                                <mi>fa</mi>
                                                <mo>&#xd7;</mo>
                                                <mi>sinC</mi>
                                            </mrow>
                                            <mrow>
                                                <mi>fc</mi>
                                            </mrow>
                                        </mfrac>
                                    </mtd>
                                </mtr>
                                <mtr>
                                    <mtd columnalign="right">
                                        <mi>sin</mi>
                                        <mfenced close=")" open="(">
                                            <mi>sAngleB</mi>
                                        </mfenced>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <mi>sinA</mi>
                                    </mtd>
                                </mtr>
                                <mtr>
                                    <mtd columnalign="right">
                                        <mi>sAngleB</mi>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <msup>
                                            <mi>sin</mi>
                                            <mn>-1</mn>
                                        </msup>
                                        <mfenced close=")" open="(">
                                            <mi>sinA</mi>
                                        </mfenced>
                                    </mtd>
                                </mtr>
                                <mtr>
                                    <mtd columnalign="right">
                                        <mi>sAngleB</mi>
                                    </mtd>
                                    <mtd columnalign="center">
                                        <mo>=</mo>
                                    </mtd>
                                    <mtd columnalign="left">
                                        <mi>fAns</mi>
                                        <mo>&#xb0;</mo>
                                    </mtd>
                                </mtr>
                            </mtable>
                            <annotation encoding="SnuggleTeX"
                                >\begin{eqnarray*}
                                \frac{\qv{sSidea}}{\sin(\qv{sAngleA})}&amp;=&amp;\frac{\qv{sSideb}}{\sin(\qv{sAngleB})}\\
                                \frac{\qv{fc}}{\sin(\qv{fC})}&amp;=&amp;\frac{\qv{fa}}{\sin(\qv{sAngleB})}\\
                                \sin(\qv{sAngleB})&amp;=&amp;\frac{\qv{fa}\times\sin(\qv{fC})}{\qv(fc)}\\
                                \sin(\qv{sAngleB})&amp;=&amp;\frac{\qv{fa}\times\qv{sinC}}{\qv{fc}}\\
                                \sin(\qv{sAngleB})&amp;=&amp;\qv{sinA}\\
                                \qv{sAngleB}&amp;=&amp;\asin{\qv{sinA}}\\
                                \qv{sAngleB}&amp;=&amp;\qv{fAns} \end{eqnarray*}</annotation>
                        </semantics>
                    </math>
                </p>
            </feedbackBlock></div>
        <div class="">
            <feedbackBlock identifier="HINT" outcomeIdentifier="FEEDBACK" showHide="show">
                <p>Use the sine rule:</p>
                <math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
                    <semantics>
                        <mrow>
                            <mfrac>
                                <mi>a</mi>
                                <mrow>
                                    <mi>sin</mi>
                                    <mfenced close=")" open="(">
                                        <mi>A</mi>
                                    </mfenced>
                                </mrow>
                            </mfrac>
                            <mo>=</mo>
                            <mfrac>
                                <mi>b</mi>
                                <mrow>
                                    <mi>sin</mi>
                                    <mfenced close=")" open="(">
                                        <mi>B</mi>
                                    </mfenced>
                                </mrow>
                            </mfrac>
                            <mo>=</mo>
                            <mfrac>
                                <mi>c</mi>
                                <mrow>
                                    <mi>sin</mi>
                                    <mfenced close=")" open="(">
                                        <mi>C</mi>
                                    </mfenced>
                                </mrow>
                            </mfrac>
                        </mrow>
                        <annotation encoding="SnuggleTeX"
                            >\[\frac{a}{\sin(A)}=\frac{b}{\sin(B)}=\frac{c}{\sin(C)}\]</annotation>
                    </semantics>
                </math>

            </feedbackBlock></div>
        <feedbackBlock identifier="SEEN-SOLUTION" outcomeIdentifier="FEEDBACK" showHide="show">
            <p> Since you have viewed the solution, your score for this question will be 0. </p>
        </feedbackBlock>
        <feedbackBlock identifier="SEEN-HINT" outcomeIdentifier="FEEDBACK" showHide="show">
            <p> Since you have viewed the hint, your score for this question will be halved. </p>
        </feedbackBlock>
        <feedbackBlock identifier="askhint" outcomeIdentifier="ASKHINT" showHide="show">
            <div>
                <endAttemptInteraction id="endAttemptInteraction1" responseIdentifier="HINTREQUEST"
                    title="Show Hint"/>
            </div>
        </feedbackBlock>
        <feedbackBlock identifier="asksolution" outcomeIdentifier="ASKSOLUTION" showHide="show">
            <div>
                <endAttemptInteraction id="endAttemptInteraction2" responseIdentifier="SOLREQUEST"
                    title="Show Solution"/>
            </div>
        </feedbackBlock>
    </itemBody>
    <responseProcessing>
        <setOutcomeValue identifier="FEEDBACK">
            <multiple>
                <variable identifier="EMPTY"/>
            </multiple>
        </setOutcomeValue>
        <responseCondition>
            <responseIf>
                <variable identifier="HINTREQUEST"/>
                <setOutcomeValue identifier="FEEDBACK">
                    <multiple>
                        <baseValue baseType="identifier">HINT</baseValue>
                    </multiple>
                </setOutcomeValue>
                <setOutcomeValue identifier="seenHint">
                    <baseValue baseType="boolean">true</baseValue>
                </setOutcomeValue>
            </responseIf>
            <responseElseIf>
                <variable identifier="SOLREQUEST"/>
                <setOutcomeValue identifier="FEEDBACK">
                    <multiple>
                        <baseValue baseType="identifier">SOLUTION</baseValue>
                    </multiple>
